{
  "type": "object",
  "properties": {
    "apiVersion": {
      "type": "string"
    },
    "kind": {
      "type": "string"
    },
    "metadata": {
      "type": "object"
    },
    "spec": {
      "type": "object",
      "required": [
        "availabilityZones",
        "channelGroup",
        "installerRoleARN",
        "oidcID",
        "region",
        "rolesRef",
        "rosaClusterName",
        "subnets",
        "supportRoleARN",
        "version",
        "versionGate",
        "workerRoleARN"
      ],
      "properties": {
        "additionalTags": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          }
        },
        "auditLogRoleARN": {
          "type": "string"
        },
        "availabilityZones": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "billingAccount": {
          "type": "string",
          "x-kubernetes-validations": [
            {
              "rule": "self == oldSelf",
              "message": "billingAccount is immutable"
            },
            {
              "rule": "self.matches('^[0-9]{12}$')",
              "message": "billingAccount must be a valid AWS account ID"
            }
          ]
        },
        "channelGroup": {
          "type": "string",
          "default": "stable",
          "enum": [
            "stable",
            "eus",
            "fast",
            "candidate",
            "nightly"
          ]
        },
        "clusterRegistryConfig": {
          "type": "object",
          "properties": {
            "additionalTrustedCAs": {
              "type": "object",
              "additionalProperties": {
                "type": "string"
              }
            },
            "allowedRegistriesForImport": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "domainName": {
                    "type": "string"
                  },
                  "insecure": {
                    "type": "boolean",
                    "default": false
                  }
                },
                "additionalProperties": false
              }
            },
            "registrySources": {
              "type": "object",
              "properties": {
                "allowedRegistries": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  }
                },
                "blockedRegistries": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  }
                },
                "insecureRegistries": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  }
                }
              },
              "additionalProperties": false
            }
          },
          "additionalProperties": false
        },
        "controlPlaneEndpoint": {
          "type": "object",
          "required": [
            "host",
            "port"
          ],
          "properties": {
            "host": {
              "type": "string",
              "maxLength": 512
            },
            "port": {
              "type": "integer",
              "format": "int32"
            }
          },
          "additionalProperties": false
        },
        "credentialsSecretRef": {
          "type": "object",
          "properties": {
            "name": {
              "type": "string",
              "default": ""
            }
          },
          "additionalProperties": false,
          "x-kubernetes-map-type": "atomic"
        },
        "defaultMachinePoolSpec": {
          "type": "object",
          "properties": {
            "autoscaling": {
              "type": "object",
              "properties": {
                "maxReplicas": {
                  "type": "integer",
                  "minimum": 1
                },
                "minReplicas": {
                  "type": "integer",
                  "minimum": 1
                }
              },
              "additionalProperties": false
            },
            "instanceType": {
              "type": "string"
            },
            "volumeSize": {
              "type": "integer",
              "maximum": 16384,
              "minimum": 75
            }
          },
          "additionalProperties": false
        },
        "domainPrefix": {
          "type": "string",
          "maxLength": 15,
          "pattern": "^[a-z]([-a-z0-9]*[a-z0-9])?$",
          "x-kubernetes-validations": [
            {
              "rule": "self == oldSelf",
              "message": "domainPrefix is immutable"
            }
          ]
        },
        "enableExternalAuthProviders": {
          "type": "boolean",
          "default": false,
          "x-kubernetes-validations": [
            {
              "rule": "self == oldSelf",
              "message": "enableExternalAuthProviders is immutable"
            }
          ]
        },
        "endpointAccess": {
          "type": "string",
          "default": "Public",
          "enum": [
            "Public",
            "Private"
          ]
        },
        "etcdEncryptionKMSARN": {
          "type": "string"
        },
        "externalAuthProviders": {
          "type": "array",
          "maxItems": 1,
          "items": {
            "type": "object",
            "required": [
              "issuer",
              "name"
            ],
            "properties": {
              "claimMappings": {
                "type": "object",
                "properties": {
                  "groups": {
                    "type": "object",
                    "required": [
                      "claim"
                    ],
                    "properties": {
                      "claim": {
                        "type": "string"
                      },
                      "prefix": {
                        "type": "string"
                      }
                    },
                    "additionalProperties": false
                  },
                  "username": {
                    "type": "object",
                    "required": [
                      "claim"
                    ],
                    "properties": {
                      "claim": {
                        "type": "string"
                      },
                      "prefix": {
                        "type": "string",
                        "minLength": 1
                      },
                      "prefixPolicy": {
                        "type": "string",
                        "enum": [
                          "",
                          "NoPrefix",
                          "Prefix"
                        ]
                      }
                    },
                    "additionalProperties": false,
                    "x-kubernetes-validations": [
                      {
                        "rule": "self.prefixPolicy == 'Prefix' ? has(self.prefix) : !has(self.prefix)",
                        "message": "prefix must be set if prefixPolicy is 'Prefix', but must remain unset otherwise"
                      }
                    ]
                  }
                },
                "additionalProperties": false
              },
              "claimValidationRules": {
                "type": "array",
                "items": {
                  "type": "object",
                  "required": [
                    "requiredClaim",
                    "type"
                  ],
                  "properties": {
                    "requiredClaim": {
                      "type": "object",
                      "required": [
                        "claim",
                        "requiredValue"
                      ],
                      "properties": {
                        "claim": {
                          "type": "string",
                          "minLength": 1
                        },
                        "requiredValue": {
                          "type": "string",
                          "minLength": 1
                        }
                      },
                      "additionalProperties": false
                    },
                    "type": {
                      "type": "string",
                      "default": "RequiredClaim",
                      "enum": [
                        "RequiredClaim"
                      ]
                    }
                  },
                  "additionalProperties": false
                },
                "x-kubernetes-list-type": "atomic"
              },
              "issuer": {
                "type": "object",
                "required": [
                  "audiences",
                  "issuerURL"
                ],
                "properties": {
                  "audiences": {
                    "type": "array",
                    "maxItems": 10,
                    "minItems": 1,
                    "items": {
                      "type": "string",
                      "minLength": 1
                    },
                    "x-kubernetes-list-type": "set"
                  },
                  "issuerCertificateAuthority": {
                    "type": "object",
                    "required": [
                      "name"
                    ],
                    "properties": {
                      "name": {
                        "type": "string"
                      }
                    },
                    "additionalProperties": false
                  },
                  "issuerURL": {
                    "type": "string",
                    "pattern": "^https:\\/\\/[^\\s]"
                  }
                },
                "additionalProperties": false
              },
              "name": {
                "type": "string",
                "minLength": 1
              },
              "oidcClients": {
                "type": "array",
                "maxItems": 20,
                "items": {
                  "type": "object",
                  "required": [
                    "clientID",
                    "clientSecret",
                    "componentName",
                    "componentNamespace"
                  ],
                  "properties": {
                    "clientID": {
                      "type": "string",
                      "minLength": 1
                    },
                    "clientSecret": {
                      "type": "object",
                      "required": [
                        "name"
                      ],
                      "properties": {
                        "name": {
                          "type": "string"
                        }
                      },
                      "additionalProperties": false
                    },
                    "componentName": {
                      "type": "string",
                      "maxLength": 256,
                      "minLength": 1
                    },
                    "componentNamespace": {
                      "type": "string",
                      "maxLength": 63,
                      "minLength": 1
                    },
                    "extraScopes": {
                      "type": "array",
                      "items": {
                        "type": "string"
                      },
                      "x-kubernetes-list-type": "set"
                    }
                  },
                  "additionalProperties": false
                },
                "x-kubernetes-list-map-keys": [
                  "componentNamespace",
                  "componentName"
                ],
                "x-kubernetes-list-type": "map"
              }
            },
            "additionalProperties": false
          },
          "x-kubernetes-list-map-keys": [
            "name"
          ],
          "x-kubernetes-list-type": "map"
        },
        "identityRef": {
          "type": "object",
          "required": [
            "kind",
            "name"
          ],
          "properties": {
            "kind": {
              "type": "string",
              "enum": [
                "AWSClusterControllerIdentity",
                "AWSClusterRoleIdentity",
                "AWSClusterStaticIdentity"
              ]
            },
            "name": {
              "type": "string",
              "minLength": 1
            }
          },
          "additionalProperties": false
        },
        "installerRoleARN": {
          "type": "string"
        },
        "network": {
          "type": "object",
          "properties": {
            "hostPrefix": {
              "type": "integer",
              "default": 23
            },
            "machineCIDR": {
              "type": "string",
              "format": "cidr"
            },
            "networkType": {
              "type": "string",
              "default": "OVNKubernetes",
              "enum": [
                "OVNKubernetes",
                "Other"
              ]
            },
            "podCIDR": {
              "type": "string",
              "format": "cidr"
            },
            "serviceCIDR": {
              "type": "string",
              "format": "cidr"
            }
          },
          "additionalProperties": false
        },
        "oidcID": {
          "type": "string",
          "x-kubernetes-validations": [
            {
              "rule": "self == oldSelf",
              "message": "oidcID is immutable"
            }
          ]
        },
        "provisionShardID": {
          "type": "string",
          "x-kubernetes-validations": [
            {
              "rule": "self == oldSelf",
              "message": "provisionShardID is immutable"
            }
          ]
        },
        "region": {
          "type": "string"
        },
        "rolesRef": {
          "type": "object",
          "required": [
            "controlPlaneOperatorARN",
            "imageRegistryARN",
            "ingressARN",
            "kmsProviderARN",
            "kubeCloudControllerARN",
            "networkARN",
            "nodePoolManagementARN",
            "storageARN"
          ],
          "properties": {
            "controlPlaneOperatorARN": {
              "type": "string"
            },
            "imageRegistryARN": {
              "type": "string"
            },
            "ingressARN": {
              "type": "string"
            },
            "kmsProviderARN": {
              "type": "string"
            },
            "kubeCloudControllerARN": {
              "type": "string"
            },
            "networkARN": {
              "type": "string"
            },
            "nodePoolManagementARN": {
              "type": "string"
            },
            "storageARN": {
              "type": "string"
            }
          },
          "additionalProperties": false
        },
        "rosaClusterName": {
          "type": "string",
          "maxLength": 54,
          "pattern": "^[a-z]([-a-z0-9]*[a-z0-9])?$",
          "x-kubernetes-validations": [
            {
              "rule": "self == oldSelf",
              "message": "rosaClusterName is immutable"
            }
          ]
        },
        "subnets": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "supportRoleARN": {
          "type": "string"
        },
        "version": {
          "type": "string"
        },
        "versionGate": {
          "type": "string",
          "default": "WaitForAcknowledge",
          "enum": [
            "Acknowledge",
            "WaitForAcknowledge",
            "AlwaysAcknowledge"
          ]
        },
        "workerRoleARN": {
          "type": "string"
        }
      },
      "additionalProperties": false
    },
    "status": {
      "type": "object",
      "required": [
        "ready"
      ],
      "properties": {
        "availableUpgrades": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "conditions": {
          "type": "array",
          "items": {
            "type": "object",
            "required": [
              "lastTransitionTime",
              "status",
              "type"
            ],
            "properties": {
              "lastTransitionTime": {
                "type": "string",
                "format": "date-time"
              },
              "message": {
                "type": "string",
                "maxLength": 10240,
                "minLength": 1
              },
              "reason": {
                "type": "string",
                "maxLength": 256,
                "minLength": 1
              },
              "severity": {
                "type": "string",
                "maxLength": 32
              },
              "status": {
                "type": "string"
              },
              "type": {
                "type": "string",
                "maxLength": 256,
                "minLength": 1
              }
            },
            "additionalProperties": false
          }
        },
        "consoleURL": {
          "type": "string"
        },
        "externalManagedControlPlane": {
          "type": "boolean",
          "default": true
        },
        "failureMessage": {
          "type": "string"
        },
        "id": {
          "type": "string"
        },
        "initialized": {
          "type": "boolean"
        },
        "oidcEndpointURL": {
          "type": "string"
        },
        "ready": {
          "type": "boolean",
          "default": false
        },
        "version": {
          "type": "string"
        }
      },
      "additionalProperties": false
    }
  }
}
