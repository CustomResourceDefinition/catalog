{
  "type": "object",
  "required": [
    "spec"
  ],
  "properties": {
    "apiVersion": {
      "type": "string"
    },
    "kind": {
      "type": "string"
    },
    "metadata": {
      "type": "object"
    },
    "spec": {
      "type": "object",
      "required": [
        "name",
        "organization",
        "token"
      ],
      "properties": {
        "deletionPolicy": {
          "type": "string",
          "default": "retain",
          "enum": [
            "retain",
            "soft"
          ]
        },
        "name": {
          "type": "string",
          "minLength": 1
        },
        "organization": {
          "type": "string",
          "minLength": 1
        },
        "teamAccess": {
          "type": "array",
          "minItems": 1,
          "items": {
            "type": "object",
            "required": [
              "access",
              "team"
            ],
            "properties": {
              "access": {
                "type": "string",
                "enum": [
                  "admin",
                  "custom",
                  "maintain",
                  "read",
                  "write"
                ]
              },
              "custom": {
                "type": "object",
                "properties": {
                  "createWorkspace": {
                    "type": "boolean",
                    "default": false
                  },
                  "deleteWorkspace": {
                    "type": "boolean",
                    "default": false
                  },
                  "lockWorkspace": {
                    "type": "boolean",
                    "default": false
                  },
                  "moveWorkspace": {
                    "type": "boolean",
                    "default": false
                  },
                  "projectAccess": {
                    "type": "string",
                    "default": "read",
                    "enum": [
                      "delete",
                      "read",
                      "update"
                    ]
                  },
                  "runTasks": {
                    "type": "boolean"
                  },
                  "runs": {
                    "type": "string",
                    "default": "read",
                    "enum": [
                      "apply",
                      "plan",
                      "read"
                    ]
                  },
                  "sentinelMocks": {
                    "type": "string",
                    "default": "none",
                    "enum": [
                      "none",
                      "read"
                    ]
                  },
                  "stateVersions": {
                    "type": "string",
                    "default": "none",
                    "enum": [
                      "none",
                      "read",
                      "read-outputs",
                      "write"
                    ]
                  },
                  "teamManagement": {
                    "type": "string",
                    "default": "none",
                    "enum": [
                      "manage",
                      "none",
                      "read"
                    ]
                  },
                  "variables": {
                    "type": "string",
                    "default": "none",
                    "enum": [
                      "none",
                      "read",
                      "write"
                    ]
                  }
                },
                "additionalProperties": false
              },
              "team": {
                "type": "object",
                "properties": {
                  "id": {
                    "type": "string",
                    "pattern": "^team-[a-zA-Z0-9]+$"
                  },
                  "name": {
                    "type": "string",
                    "minLength": 1
                  }
                },
                "additionalProperties": false
              }
            },
            "additionalProperties": false
          }
        },
        "token": {
          "type": "object",
          "required": [
            "secretKeyRef"
          ],
          "properties": {
            "secretKeyRef": {
              "type": "object",
              "required": [
                "key"
              ],
              "properties": {
                "key": {
                  "type": "string"
                },
                "name": {
                  "type": "string",
                  "default": ""
                },
                "optional": {
                  "type": "boolean"
                }
              },
              "additionalProperties": false,
              "x-kubernetes-map-type": "atomic"
            }
          },
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    },
    "status": {
      "type": "object",
      "required": [
        "id",
        "name",
        "observedGeneration"
      ],
      "properties": {
        "id": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "observedGeneration": {
          "type": "integer",
          "format": "int64"
        }
      },
      "additionalProperties": false
    }
  }
}
