        "disabled": {
          "type": "boolean"
        },
        "nodeImageSelection": {
          "type": "object",
          "required": [
            "type"
          ],
          "properties": {
            "type": {
              "type": "string",
              "enum": [
                "Consistent",
                "Latest"
              ]
            }
          },
          "additionalProperties": false
        },
        "operatorSpec": {
          "type": "object",
          "properties": {
            "configMapExpressions": {
              "type": "array",
