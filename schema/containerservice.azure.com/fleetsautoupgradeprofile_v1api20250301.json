                      },
                      "code": {
                        "type": "string"
                      },
                      "message": {
                        "type": "string"
                      },
                      "target": {
                        "type": "string"
                      }
                    },
                    "additionalProperties": false
                  }
                },
                "message": {
                  "type": "string"
                },
                "target": {
                  "type": "string"
                }
