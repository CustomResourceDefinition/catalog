{
  "type": "object",
  "required": [
    "origin"
  ],
  "properties": {
    "alias": {
      "type": "string"
    },
    "apiVersion": {
      "type": "string"
    },
    "charts": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "repoURL": {
            "type": "string"
          },
          "version": {
            "type": "string"
          }
        },
        "additionalProperties": false
      }
    },
    "commits": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "author": {
            "type": "string"
          },
          "branch": {
            "type": "string"
          },
          "committer": {
            "type": "string"
          },
          "id": {
            "type": "string"
          },
          "message": {
            "type": "string"
          },
          "repoURL": {
            "type": "string"
          },
          "tag": {
            "type": "string"
          }
        },
        "additionalProperties": false
      }
    },
    "images": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "digest": {
            "type": "string"
          },
          "gitRepoURL": {
            "type": "string"
          },
          "repoURL": {
            "type": "string"
          },
          "tag": {
            "type": "string"
          }
        },
        "additionalProperties": false
      }
    },
    "kind": {
      "type": "string"
    },
    "metadata": {
      "type": "object"
    },
    "origin": {
      "type": "object",
      "required": [
        "kind",
        "name"
      ],
      "properties": {
        "kind": {
          "type": "string",
          "enum": [
            "Warehouse"
          ]
        },
        "name": {
          "type": "string"
        }
      },
      "additionalProperties": false
    },
    "status": {
      "type": "object",
      "properties": {
        "approvedFor": {
          "type": "object",
          "additionalProperties": {
            "description": "ApprovedStage describes a Stage for which Freight has been (manually)\napproved.",
            "type": "object"
          }
        },
        "verifiedIn": {
          "type": "object",
          "additionalProperties": {
            "description": "VerifiedStage describes a Stage in which Freight has been verified.",
            "type": "object"
          }
        }
      },
      "additionalProperties": false
    }
  }
}
