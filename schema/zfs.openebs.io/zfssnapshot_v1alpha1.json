    },
    "kind": {
      "type": "string"
    },
    "metadata": {
      "type": "object"
    },
    "spec": {
      "additionalProperties": false,
      "properties": {
        "capacity": {
          "minLength": 1,
          "type": "string"
        },
        "compression": {
          "pattern": "^(on|off|lzjb|gzip|gzip-[1-9]|zle|lz4)$",
          "type": "string"
        },
        "dedup": {
          "enum": [
            "on",
            "off"
          ],
          "type": "string"
        },
        "encryption": {
          "pattern": "^(on|off|aes-128-[c,g]cm|aes-192-[c,g]cm|aes-256-[c,g]cm)$",
          "type": "string"
        },
        "fsType": {
          "type": "string"
        },
        "keyformat": {
          "enum": [
            "passphrase",
            "raw",
            "hex"
          ],
          "type": "string"
        },
        "keylocation": {
          "type": "string"
        },
        "ownerNodeID": {
          "minLength": 1,
          "type": "string"
        },
        "poolName": {
          "minLength": 1,
          "type": "string"
        },
        "recordsize": {
          "minLength": 1,
          "type": "string"
        },
        "snapname": {
          "type": "string"
        },
        "thinProvision": {
          "enum": [
            "yes",
            "no"
          ],
          "type": "string"
        },
        "volblocksize": {
          "minLength": 1,
          "type": "string"
        },
        "volumeType": {
          "enum": [
            "ZVOL",
            "DATASET"
          ],
          "type": "string"
        }
      },
      "required": [
        "capacity",
        "ownerNodeID",
        "poolName",
        "volumeType"
      ],
      "type": "object"
    },
    "status": {
