{
  "type": "object",
  "properties": {
    "apiVersion": {
      "type": "string"
    },
    "kind": {
      "type": "string"
    },
    "metadata": {
      "type": "object"
    },
    "spec": {
      "type": "object",
      "properties": {
        "action": {
          "type": "string",
          "default": "ALLOW",
          "enum": [
            "ALLOW"
          ]
        },
        "disabled": {
          "type": "boolean"
        },
        "ingress": {
          "type": "object",
          "properties": {
            "destination": {
              "type": "object",
              "properties": {
                "ipBlocks": {
                  "type": "array",
                  "maxItems": 256,
                  "minItems": 1,
                  "items": {
                    "type": "string",
                    "pattern": "^((25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\\.){3}(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)(/(3[0-2]|2[0-9]|1[0-9]|[0-9]))?$"
                  }
                }
              },
              "additionalProperties": false
            },
            "source": {
              "type": "object",
              "properties": {
                "ipBlocks": {
                  "type": "array",
                  "maxItems": 256,
                  "minItems": 1,
                  "items": {
                    "type": "string",
                    "pattern": "^((25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\\.){3}(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)(/(3[0-2]|2[0-9]|1[0-9]|[0-9]))?$"
                  }
                }
              },
              "additionalProperties": false
            }
          },
          "additionalProperties": false
        },
        "ports": {
          "type": "array",
          "items": {
            "type": "object",
            "required": [
              "protocol"
            ],
            "properties": {
              "endPort": {
                "type": "integer",
                "format": "int32",
                "maximum": 65535,
                "minimum": 1
              },
              "protocol": {
                "type": "string",
                "enum": [
                  "TCP",
                  "UDP",
                  "ICMP",
                  "SCTP",
                  "AH",
                  "ESP"
                ]
              },
              "startPort": {
                "type": "integer",
                "format": "int32",
                "maximum": 65535,
                "minimum": 1
              }
            },
            "additionalProperties": false
          }
        }
      },
      "additionalProperties": false
    },
    "status": {
      "type": "object",
      "default": {
        "conditions": [
          {
            "lastTransitionTime": "1970-01-01T00:00:00Z",
            "message": "Waiting for controller",
            "reason": "Pending",
            "status": "Unknown",
            "type": "Enforced"
          }
        ]
      },
      "properties": {
        "conditions": {
          "type": "array",
          "default": [
            {
              "lastTransitionTime": "1970-01-01T00:00:00Z",
              "message": "Waiting for controller",
              "reason": "Pending",
              "status": "Unknown",
              "type": "Enforced"
            }
          ],
          "maxItems": 8,
          "items": {
            "type": "object",
            "required": [
              "lastTransitionTime",
              "message",
              "reason",
              "status",
              "type"
            ],
            "properties": {
              "lastTransitionTime": {
                "type": "string",
                "format": "date-time"
              },
              "message": {
                "type": "string",
                "maxLength": 32768
              },
              "observedGeneration": {
                "type": "integer",
                "format": "int64",
                "minimum": 0
              },
              "reason": {
                "type": "string",
                "maxLength": 1024,
                "minLength": 1,
                "pattern": "^[A-Za-z]([A-Za-z0-9_,:]*[A-Za-z0-9_])?$"
              },
              "status": {
                "type": "string",
                "enum": [
                  "True",
                  "False",
                  "Unknown"
                ]
              },
              "type": {
                "type": "string",
                "maxLength": 316,
                "pattern": "^([a-z0-9]([-a-z0-9]*[a-z0-9])?(\\.[a-z0-9]([-a-z0-9]*[a-z0-9])?)*/)?(([A-Za-z0-9][-A-Za-z0-9_.]*)?[A-Za-z0-9])$"
              }
            },
            "additionalProperties": false
          },
          "x-kubernetes-list-map-keys": [
            "type"
          ],
          "x-kubernetes-list-type": "map"
        },
        "priority": {
          "type": "integer",
          "format": "int32"
        },
        "resourceURL": {
          "type": "string"
        },
        "type": {
          "type": "string",
          "enum": [
            "VPC",
            "REGIONAL",
            "GLOBAL"
          ]
        }
      },
      "additionalProperties": false
    }
  }
}
