{
  "type": "object",
  "required": [
    "spec"
  ],
  "properties": {
    "apiVersion": {
      "type": "string"
    },
    "kind": {
      "type": "string"
    },
    "metadata": {
      "type": "object"
    },
    "spec": {
      "type": "object",
      "required": [
        "targetRef"
      ],
      "properties": {
        "default": {
          "type": "object",
          "required": [
            "disabled"
          ],
          "properties": {
            "bypassCacheOnRequestHeaders": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "headerName": {
                    "type": "string",
                    "maxLength": 256
                  }
                },
                "additionalProperties": false
              }
            },
            "cacheMode": {
              "type": "string",
              "enum": [
                "USE_ORIGIN_HEADERS",
                "FORCE_CACHE_ALL",
                "CACHE_ALL_STATIC"
              ]
            },
            "cachePolicy": {
              "type": "object",
              "properties": {
                "includeHost": {
                  "type": "boolean"
                },
                "includeProtocol": {
                  "type": "boolean"
                },
                "includeQueryString": {
                  "type": "boolean"
                },
                "queryStringAllowlist": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  }
                },
                "queryStringDenylist": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  }
                }
              },
              "additionalProperties": false
            },
            "clientTtl": {
              "type": "integer",
              "format": "int32",
              "maximum": 31622400,
              "minimum": 0
            },
            "defaultTtl": {
              "type": "integer",
              "format": "int32",
              "maximum": 31622400,
              "minimum": 0
            },
            "disabled": {
              "type": "boolean"
            },
            "maxTtl": {
              "type": "integer",
              "format": "int32",
              "maximum": 31622400,
              "minimum": 0
            },
            "negativeCaching": {
              "type": "boolean"
            },
            "negativeCachingPolicy": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "code": {
                    "type": "integer",
                    "format": "int32",
                    "enum": [
                      300,
                      301,
                      302,
                      307,
                      308,
                      404,
                      405,
                      410,
                      421,
                      451,
                      501
                    ]
                  },
                  "ttl": {
                    "type": "integer",
                    "format": "int32",
                    "maximum": 1800,
                    "minimum": 0
                  }
                },
                "additionalProperties": false
              }
            },
            "requestCoalescing": {
              "type": "boolean"
            },
            "serveWhileStale": {
              "type": "integer",
              "format": "int32",
              "maximum": 604800,
              "minimum": 0
            },
            "signedUrlCacheMaxAgeSec": {
              "type": "integer",
              "format": "int64",
              "minimum": 0
            }
          },
          "additionalProperties": false
        },
        "targetRef": {
          "type": "object",
          "required": [
            "group",
            "kind",
            "name"
          ],
          "properties": {
            "group": {
              "type": "string",
              "maxLength": 253,
              "pattern": "^$|^[a-z0-9]([-a-z0-9]*[a-z0-9])?(\\.[a-z0-9]([-a-z0-9]*[a-z0-9])?)*$"
            },
            "kind": {
              "type": "string",
              "maxLength": 63,
              "minLength": 1,
              "pattern": "^[a-zA-Z]([-a-zA-Z0-9]*[a-zA-Z0-9])?$"
            },
            "name": {
              "type": "string",
              "maxLength": 253,
              "minLength": 1
            },
            "namespace": {
              "type": "string",
              "maxLength": 63,
              "minLength": 1,
              "pattern": "^[a-z0-9]([-a-z0-9]*[a-z0-9])?$"
            }
          },
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    },
    "status": {
      "type": "object",
      "properties": {
        "conditions": {
          "type": "array",
          "maxItems": 8,
          "items": {
            "type": "object",
            "required": [
              "lastTransitionTime",
              "message",
              "reason",
              "status",
              "type"
            ],
            "properties": {
              "lastTransitionTime": {
                "type": "string",
                "format": "date-time"
              },
              "message": {
                "type": "string",
                "maxLength": 32768
              },
              "observedGeneration": {
                "type": "integer",
                "format": "int64",
                "minimum": 0
              },
              "reason": {
                "type": "string",
                "maxLength": 1024,
                "minLength": 1,
                "pattern": "^[A-Za-z]([A-Za-z0-9_,:]*[A-Za-z0-9_])?$"
              },
              "status": {
                "type": "string",
                "enum": [
                  "True",
                  "False",
                  "Unknown"
                ]
              },
              "type": {
                "type": "string",
                "maxLength": 316,
                "pattern": "^([a-z0-9]([-a-z0-9]*[a-z0-9])?(\\.[a-z0-9]([-a-z0-9]*[a-z0-9])?)*/)?(([A-Za-z0-9][-A-Za-z0-9_.]*)?[A-Za-z0-9])$"
              }
            },
            "additionalProperties": false
          },
          "x-kubernetes-list-map-keys": [
            "type"
          ],
          "x-kubernetes-list-type": "map"
        }
      },
      "additionalProperties": false
    }
  }
}
