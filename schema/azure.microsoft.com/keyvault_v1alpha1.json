{
  "type": "object",
  "properties": {
    "apiVersion": {
      "type": "string"
    },
    "kind": {
      "type": "string"
    },
    "metadata": {
      "type": "object"
    },
    "spec": {
      "type": "object",
      "required": [
        "location",
        "resourceGroup"
      ],
      "properties": {
        "accessPolicies": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "applicationID": {
                "type": "string"
              },
              "clientID": {
                "type": "string"
              },
              "objectID": {
                "type": "string"
              },
              "permissions": {
                "type": "object",
                "properties": {
                  "certificates": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  },
                  "keys": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  },
                  "secrets": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  },
                  "storage": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  }
                },
                "additionalProperties": false
              },
              "tenantID": {
                "type": "string"
              }
            },
            "additionalProperties": false
          }
        },
        "enableSoftDelete": {
          "type": "boolean"
        },
        "location": {
          "type": "string"
        },
        "networkPolicies": {
          "type": "object",
          "properties": {
            "bypass": {
              "type": "string"
            },
            "defaultAction": {
              "type": "string"
            },
            "ipRules": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "virtualNetworkRules": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          },
          "additionalProperties": false
        },
        "resourceGroup": {
          "type": "string",
          "minLength": 1,
          "pattern": "^[-\\w\\._\\(\\)]+$"
        },
        "sku": {
          "type": "object",
          "properties": {
            "name": {
              "type": "string"
            }
          },
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    },
    "status": {
      "type": "object",
      "properties": {
        "completed": {
          "type": "string",
          "format": "date-time"
        },
        "containsUpdate": {
          "type": "boolean"
        },
        "failedProvisioning": {
          "type": "boolean"
        },
        "flattenedSecrets": {
          "type": "boolean"
        },
        "message": {
          "type": "string"
        },
        "output": {
          "type": "string"
        },
        "pollingUrl": {
          "type": "string"
        },
        "pollingUrlKind": {
          "type": "string",
          "enum": [
            "CreateOrUpdate",
            "Delete"
          ]
        },
        "provisioned": {
          "type": "boolean"
        },
        "provisioning": {
          "type": "boolean"
        },
        "requested": {
          "type": "string",
          "format": "date-time"
        },
        "resourceId": {
          "type": "string"
        },
        "specHash": {
          "type": "string"
        },
        "state": {
          "type": "string"
        }
      },
      "additionalProperties": false
    }
  }
}
