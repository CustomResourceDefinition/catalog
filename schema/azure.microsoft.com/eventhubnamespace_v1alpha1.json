{
  "type": "object",
  "properties": {
    "apiVersion": {
      "type": "string"
    },
    "kind": {
      "type": "string"
    },
    "metadata": {
      "type": "object"
    },
    "spec": {
      "type": "object",
      "required": [
        "location",
        "resourceGroup"
      ],
      "properties": {
        "location": {
          "type": "string"
        },
        "networkRule": {
          "type": "object",
          "properties": {
            "defaultAction": {
              "type": "string"
            },
            "ipRules": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "ipMask": {
                    "type": "string"
                  }
                },
                "additionalProperties": false
              }
            },
            "virtualNetworkRules": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "ignoreMissingServiceEndpoint": {
                    "type": "boolean"
                  },
                  "subnetId": {
                    "type": "string"
                  }
                },
                "additionalProperties": false
              }
            }
          },
          "additionalProperties": false
        },
        "properties": {
          "type": "object",
          "properties": {
            "isAutoInflateEnabled": {
              "type": "boolean"
            },
            "kafkaEnabled": {
              "type": "boolean"
            },
            "maximumThroughputUnits": {
              "type": "integer",
              "format": "int32"
            }
          },
          "additionalProperties": false
        },
        "resourceGroup": {
          "type": "string",
          "minLength": 1,
          "pattern": "^[-\\w\\._\\(\\)]+$"
        },
        "sku": {
          "type": "object",
          "properties": {
            "capacity": {
              "type": "integer",
              "format": "int32"
            },
            "name": {
              "type": "string"
            },
            "tier": {
              "type": "string"
            }
          },
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    },
    "status": {
      "type": "object",
      "properties": {
        "completed": {
          "type": "string",
          "format": "date-time"
        },
        "containsUpdate": {
          "type": "boolean"
        },
        "failedProvisioning": {
          "type": "boolean"
        },
        "flattenedSecrets": {
          "type": "boolean"
        },
        "message": {
          "type": "string"
        },
        "output": {
          "type": "string"
        },
        "pollingUrl": {
          "type": "string"
        },
        "pollingUrlKind": {
          "type": "string",
          "enum": [
            "CreateOrUpdate",
            "Delete"
          ]
        },
        "provisioned": {
          "type": "boolean"
        },
        "provisioning": {
          "type": "boolean"
        },
        "requested": {
          "type": "string",
          "format": "date-time"
        },
        "resourceId": {
          "type": "string"
        },
        "specHash": {
          "type": "string"
        },
        "state": {
          "type": "string"
        }
      },
      "additionalProperties": false
    }
  }
}
