                        },
                        {
                          "required": [
                            "subject",
                            "op",
                            "set"
                          ]
                        }
                      ],
                      "properties": {
                        "op": {
                          "enum": [
                            "Equal",
                            "NotEqual",
                            "GreaterThan",
                            "LessThan",
                            "In",
                            "NotIn",
                            "RegexMatch",
                            "RegexNotMatch",
                            "RegexMatchCaseInsensitive",
                            "RegexNotMatchCaseInsensitive"
