                    "enum": [
                      "basicAuth",
                      "keyAuth"
                    ],
                    "type": "string"
                  }
                },
                "required": [
                  "enable"
                ],
                "type": "object"
              },
              "backends": {
                "items": {
                  "additionalProperties": false,
                  "properties": {
                    "resolveGranularity": {
                      "enum": [
                        "endpoint",
                        "service"
                      ],
                      "type": "string"
                    },
                    "serviceName": {
                      "minLength": 1,
                      "type": "string"
                    },
                    "servicePort": {
                      "anyOf": [
                        {
                          "type": "integer"
                        },
                        {
                          "type": "string"
                        }
                      ],
                      "x-kubernetes-int-or-string": true
                    },
                    "subset": {
                      "type": "string"
                    },
                    "weight": {
                      "minimum": 0,
                      "type": "integer"
                    }
                  },
                  "type": "object"
                },
                "minItems": 1,
                "required": [
                  "serviceName",
                  "servicePort"
