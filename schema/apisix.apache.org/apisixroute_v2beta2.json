                              "type": "string"
                            },
                            "scope": {
                              "enum": [
                                "Cookie",
                                "Header",
                                "Path",
                                "Query"
                              ],
                              "type": "string"
                            }
                          },
                          "required": [
                            "scope"
                          ],
                          "type": "object"
                        },
                        "value": {
                          "type": "string"
                        }
                      },
                      "type": "object"
                    },
                    "minItems": 1,
                    "type": "array"
                  },
                  "hosts": {
                    "items": {
                      "pattern": "^\\*?[0-9a-zA-Z-._]+$",
                      "type": "string"
                    },
                    "minItems": 1,
                    "type": "array"
                  },
                  "methods": {
                    "items": {
                      "enum": [
                        "CONNECT",
                        "DELETE",
                        "GET",
                        "HEAD",
                        "OPTIONS",
                        "PATCH",
                        "POST",
                        "PUT",
                        "TRACE"
                      ],
                      "type": "string"
                    },
                    "minItems": 1,
                    "type": "array"
                  },
                  "paths": {
                    "items": {
                      "pattern": "^/[a-zA-Z0-9\\-._~%!$&'()+,;=:@/]*\\*?$",
                      "type": "string"
                    },
                    "minItems": 1,
                    "type": "array"
                  },
                  "remoteAddrs": {
                    "items": {
                      "type": "string"
                    },
                    "minItems": 1,
                    "type": "array"
                  }
                },
                "required": [
                  "paths"
                ],
                "type": "object"
              },
              "name": {
                "minLength": 1,
