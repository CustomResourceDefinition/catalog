                "type": "array"
              },
              "match": {
                "additionalProperties": false,
                "properties": {
                  "exprs": {
                    "items": {
                      "additionalProperties": false,
                      "oneOf": [
                        {
                          "required": [
                            "subject",
                            "op",
                            "value"
