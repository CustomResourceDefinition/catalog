                  "deleteOnTermination": {
                    "type": "boolean"
                  },
                  "encrypted": {
                    "type": "boolean"
                  },
                  "iops": {
                    "type": "integer",
                    "format": "int64"
                  },
                  "kmsKeyID": {
                    "type": "string"
                  },
                  "snapshotID": {
                    "type": "string"
                  },
                  "throughput": {
                    "type": "integer",
                    "format": "int64"
                  },
                  "volumeSize": {
                    "pattern": "^(\\+|-)?(([0-9]+(\\.[0-9]*)?)|(\\.[0-9]+))(([KMGTPE]i)|[numkMGTPE]|([eE](\\+|-)?(([0-9]+(\\.[0-9]*)?)|(\\.[0-9]+))))?$",
                    "anyOf": [
                      {
                        "type": "integer"
                      },
                      {
                        "type": "string"
                      }
                    ],
                    "x-kubernetes-int-or-string": true
                  },
                  "volumeType": {
                    "type": "string"
                  }
                },
                "additionalProperties": false
              }
            },
            "additionalProperties": false
          }
        },
        "context": {
          "type": "string"
        },
        "detailedMonitoring": {
          "type": "boolean"
        },
        "instanceProfile": {
          "type": "string"
        },
        "kind": {
          "type": "string"
        },
        "launchTemplate": {
          "type": "string"
        },
        "metadataOptions": {
          "type": "object",
          "properties": {
            "httpEndpoint": {
              "type": "string"
            },
            "httpProtocolIPv6": {
              "type": "string"
            },
            "httpPutResponseHopLimit": {
              "type": "integer",
              "format": "int64"
            },
            "httpTokens": {
              "type": "string"
            }
          },
          "additionalProperties": false
        },
        "securityGroupSelector": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          }
        },
        "subnetSelector": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          }
        },
        "tags": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          }
        },
        "userData": {
          "type": "string"
        }
      },
      "additionalProperties": false
    },
    "status": {
      "type": "object",
      "properties": {
        "amis": {
          "type": "array",
          "items": {
            "type": "object",
            "required": [
              "id",
              "requirements"
            ],
            "properties": {
              "id": {
                "type": "string"
              },
              "name": {
                "type": "string"
              },
              "requirements": {
                "type": "array",
                "items": {
                  "type": "object",
