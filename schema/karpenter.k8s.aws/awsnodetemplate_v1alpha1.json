      "type": "string"
    },
    "kind": {
      "type": "string"
    },
    "metadata": {
      "type": "object"
    },
    "spec": {
      "type": "object",
      "properties": {
        "amiFamily": {
          "type": "string"
        },
        "amiSelector": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          }
        },
        "apiVersion": {
          "type": "string"
        },
        "blockDeviceMappings": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "deviceName": {
                "type": "string"
              },
              "ebs": {
                "type": "object",
