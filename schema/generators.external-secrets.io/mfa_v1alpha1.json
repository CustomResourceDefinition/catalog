{
  "properties": {
    "apiVersion": {
      "type": "string"
    },
    "kind": {
      "type": "string"
    },
    "metadata": {
      "type": "object"
    },
    "spec": {
      "additionalProperties": false,
      "properties": {
        "algorithm": {
          "type": "string"
        },
        "length": {
          "type": "integer"
        },
        "secret": {
          "additionalProperties": false,
          "properties": {
            "key": {
              "maxLength": 253,
              "minLength": 1,
              "pattern": "^[-._a-zA-Z0-9]+$",
              "type": "string"
            },
            "name": {
              "maxLength": 253,
              "minLength": 1,
              "pattern": "^[a-z0-9]([-a-z0-9]*[a-z0-9])?(\\.[a-z0-9]([-a-z0-9]*[a-z0-9])?)*$",
              "type": "string"
            },
            "namespace": {
              "maxLength": 63,
              "minLength": 1,
              "pattern": "^[a-z0-9]([-a-z0-9]*[a-z0-9])?$",
              "type": "string"
            }
          },
          "type": "object"
        },
        "timePeriod": {
          "type": "integer"
        },
        "when": {
          "format": "date-time",
          "type": "string"
        }
      },
      "required": [
        "secret"
      ],
      "type": "object"
    }
  },
  "type": "object"
