{
  "type": "object",
  "properties": {
    "apiVersion": {
      "type": "string"
    },
    "kind": {
      "type": "string"
    },
    "metadata": {
      "type": "object"
    },
    "spec": {
      "type": "object",
      "required": [
        "endpointType",
        "project"
      ],
      "properties": {
        "authSecretRef": {
          "type": "object",
          "required": [
            "key",
            "name"
          ],
          "properties": {
            "key": {
              "type": "string",
              "minLength": 1
            },
            "name": {
              "type": "string",
              "minLength": 1
            }
          },
          "additionalProperties": false
        },
        "autoscaler": {
          "type": "object",
          "required": [
            "autoscaling"
          ],
          "properties": {
            "autoscaling": {
              "type": "array",
              "maxItems": 64,
              "items": {
                "type": "object",
                "required": [
                  "cap_gb",
                  "type"
                ],
                "properties": {
                  "cap_gb": {
                    "type": "integer",
                    "minimum": 50
                  },
                  "type": {
                    "type": "string",
                    "enum": [
                      "autoscale_disk"
                    ]
                  }
                },
                "additionalProperties": false
              }
            }
          },
          "additionalProperties": false
        },
        "datadog": {
          "type": "object",
          "required": [
            "datadog_api_key"
          ],
          "properties": {
            "datadog_api_key": {
              "type": "string",
              "maxLength": 256,
              "minLength": 1,
              "pattern": "^[A-Za-z0-9]{1,256}$"
            },
            "datadog_tags": {
              "type": "array",
              "maxItems": 32,
              "items": {
                "type": "object",
                "required": [
                  "tag"
                ],
                "properties": {
                  "comment": {
                    "type": "string",
                    "maxLength": 1024
                  },
                  "tag": {
                    "type": "string",
                    "maxLength": 200,
                    "minLength": 1
                  }
                },
                "additionalProperties": false
              }
            },
            "disable_consumer_stats": {
              "type": "boolean"
            },
            "extra_tags_prefix": {
              "type": "string",
              "maxLength": 64,
              "minLength": 0,
              "pattern": "^[A-Za-z0-9\\-]{0,64}$"
            },
            "kafka_consumer_check_instances": {
              "type": "integer",
              "maximum": 100,
              "minimum": 1
            },
            "kafka_consumer_stats_timeout": {
              "type": "integer",
              "maximum": 300,
              "minimum": 2
            },
            "max_partition_contexts": {
              "type": "integer",
              "maximum": 200000,
              "minimum": 200
            },
            "site": {
              "type": "string",
              "enum": [
                "ap1.datadoghq.com",
                "datadoghq.com",
                "datadoghq.eu",
                "ddog-gov.com",
                "us3.datadoghq.com",
                "us5.datadoghq.com"
              ]
            }
          },
          "additionalProperties": false
        },
        "endpointName": {
          "type": "string",
          "maxLength": 36,
          "x-kubernetes-validations": [
            {
              "rule": "self == oldSelf",
              "message": "Value is immutable"
            }
          ]
        },
        "endpointType": {
          "type": "string",
          "enum": [
            "autoscaler",
            "datadog",
            "external_aws_cloudwatch_logs",
            "external_aws_cloudwatch_metrics",
            "external_aws_s3",
            "external_clickhouse",
            "external_elasticsearch_logs",
            "external_google_cloud_bigquery",
            "external_google_cloud_logging",
            "external_kafka",
            "external_mysql",
            "external_opensearch_logs",
            "external_postgresql",
            "external_redis",
            "external_schema_registry",
            "external_sumologic_logs",
            "jolokia",
            "prometheus",
            "rsyslog"
          ],
          "x-kubernetes-validations": [
            {
              "rule": "self == oldSelf",
              "message": "Value is immutable"
            }
          ]
        },
        "externalAWSCloudwatchLogs": {
          "type": "object",
          "required": [
            "access_key",
            "region",
            "secret_key"
          ],
          "properties": {
            "access_key": {
              "type": "string",
              "maxLength": 4096
            },
            "log_group_name": {
              "type": "string",
              "maxLength": 512,
              "minLength": 1,
              "pattern": "^[\\.\\-_/#A-Za-z0-9]+$"
            },
            "region": {
              "type": "string",
              "maxLength": 32
            },
            "secret_key": {
              "type": "string",
              "maxLength": 4096
            }
          },
          "additionalProperties": false
        },
        "externalAWSCloudwatchMetrics": {
          "type": "object",
          "required": [
            "access_key",
            "namespace",
            "region",
            "secret_key"
          ],
          "properties": {
            "access_key": {
              "type": "string",
              "maxLength": 4096
            },
            "namespace": {
              "type": "string",
              "maxLength": 255,
              "minLength": 1
            },
            "region": {
              "type": "string",
              "maxLength": 32
            },
            "secret_key": {
              "type": "string",
              "maxLength": 4096
            }
          },
          "additionalProperties": false
        },
        "externalElasticsearchLogs": {
          "type": "object",
          "required": [
            "index_prefix",
            "url"
          ],
          "properties": {
            "ca": {
              "type": "string",
              "maxLength": 16384
            },
            "index_days_max": {
              "type": "integer",
              "maximum": 10000,
              "minimum": 1
            },
            "index_prefix": {
              "type": "string",
              "maxLength": 1000,
              "minLength": 1,
              "pattern": "^[a-z0-9][a-z0-9-_.]+$"
            },
            "timeout": {
              "type": "number",
              "maximum": 120,
              "minimum": 10
            },
            "url": {
              "type": "string",
              "maxLength": 2048,
              "minLength": 12
            }
          },
          "additionalProperties": false
        },
        "externalGoogleCloudBigquery": {
          "type": "object",
          "required": [
            "project_id",
            "service_account_credentials"
          ],
          "properties": {
            "project_id": {
              "type": "string",
              "maxLength": 30,
              "minLength": 6
            },
            "service_account_credentials": {
              "type": "string",
              "maxLength": 4096
            }
          },
          "additionalProperties": false
        },
        "externalGoogleCloudLogging": {
          "type": "object",
          "required": [
            "log_id",
            "project_id",
            "service_account_credentials"
          ],
          "properties": {
            "log_id": {
              "type": "string",
              "maxLength": 512
            },
            "project_id": {
              "type": "string",
              "maxLength": 30,
              "minLength": 6
            },
            "service_account_credentials": {
              "type": "string",
              "maxLength": 4096
            }
          },
          "additionalProperties": false
        },
        "externalKafka": {
          "type": "object",
          "required": [
            "bootstrap_servers",
            "security_protocol"
          ],
          "properties": {
            "bootstrap_servers": {
              "type": "string",
              "maxLength": 256,
              "minLength": 3
            },
            "sasl_mechanism": {
              "type": "string",
              "enum": [
                "PLAIN",
                "SCRAM-SHA-256",
                "SCRAM-SHA-512"
              ]
            },
            "sasl_plain_password": {
              "type": "string",
              "maxLength": 256,
              "minLength": 1
            },
            "sasl_plain_username": {
              "type": "string",
              "maxLength": 256,
              "minLength": 1
            },
            "security_protocol": {
              "type": "string",
              "enum": [
                "PLAINTEXT",
                "SASL_PLAINTEXT",
                "SASL_SSL",
                "SSL"
              ]
            },
            "ssl_ca_cert": {
              "type": "string",
              "maxLength": 16384
            },
            "ssl_client_cert": {
              "type": "string",
              "maxLength": 16384
            },
            "ssl_client_key": {
              "type": "string",
              "maxLength": 16384
            },
            "ssl_endpoint_identification_algorithm": {
              "type": "string",
              "enum": [
                "https"
              ]
            }
          },
          "additionalProperties": false
        },
        "externalOpensearchLogs": {
          "type": "object",
          "required": [
            "index_prefix",
            "url"
          ],
          "properties": {
            "ca": {
              "type": "string",
              "maxLength": 16384
            },
            "index_days_max": {
              "type": "integer",
              "maximum": 10000,
              "minimum": 1
            },
            "index_prefix": {
              "type": "string",
              "maxLength": 1000,
              "minLength": 1,
              "pattern": "^[a-z0-9][a-z0-9-_.]+$"
            },
            "timeout": {
              "type": "number",
              "maximum": 120,
              "minimum": 10
            },
            "url": {
              "type": "string",
              "maxLength": 2048,
              "minLength": 12
            }
          },
          "additionalProperties": false
        },
        "externalPostgresql": {
          "type": "object",
          "required": [
            "host",
            "port",
            "username"
          ],
          "properties": {
            "default_database": {
              "type": "string",
              "maxLength": 63,
              "pattern": "^[_A-Za-z0-9][-_A-Za-z0-9]{0,62}$"
            },
            "host": {
              "type": "string",
              "maxLength": 255
            },
            "password": {
              "type": "string",
              "maxLength": 256
            },
            "port": {
              "type": "integer",
              "maximum": 65535,
              "minimum": 1
            },
            "ssl_client_certificate": {
              "type": "string",
              "maxLength": 16384
            },
            "ssl_client_key": {
              "type": "string",
              "maxLength": 16384
            },
            "ssl_mode": {
              "type": "string",
              "enum": [
                "prefer",
                "require",
                "verify-ca",
                "verify-full"
              ]
            },
            "ssl_root_cert": {
              "type": "string",
              "maxLength": 16384
            },
            "username": {
              "type": "string",
              "maxLength": 256
            }
          },
          "additionalProperties": false
        },
        "externalSchemaRegistry": {
          "type": "object",
          "required": [
            "authentication",
            "url"
          ],
          "properties": {
            "authentication": {
              "type": "string",
              "enum": [
                "basic",
                "none"
              ]
            },
            "basic_auth_password": {
              "type": "string",
              "maxLength": 256
            },
            "basic_auth_username": {
              "type": "string",
              "maxLength": 256
            },
            "url": {
              "type": "string",
              "maxLength": 2048
            }
          },
          "additionalProperties": false
        },
        "jolokia": {
          "type": "object",
          "properties": {
            "basic_auth_password": {
              "type": "string",
              "maxLength": 64,
              "minLength": 8
            },
            "basic_auth_username": {
              "type": "string",
              "maxLength": 32,
              "minLength": 5,
              "pattern": "^[a-z0-9\\-@_]{5,32}$"
            }
          },
          "additionalProperties": false
        },
        "project": {
          "type": "string",
          "maxLength": 63,
          "pattern": "^[a-zA-Z0-9_-]+$",
          "x-kubernetes-validations": [
            {
              "rule": "self == oldSelf",
              "message": "Value is immutable"
            }
          ]
        },
        "prometheus": {
          "type": "object",
          "properties": {
            "basic_auth_password": {
              "type": "string",
              "maxLength": 256,
              "minLength": 8
            },
            "basic_auth_username": {
              "type": "string",
              "maxLength": 32,
              "minLength": 5,
              "pattern": "^[a-z0-9\\-@_]{5,32}$"
            }
          },
          "additionalProperties": false
        },
        "rsyslog": {
          "type": "object",
          "required": [
            "format",
            "port",
            "server",
            "tls"
          ],
          "properties": {
            "ca": {
              "type": "string",
              "maxLength": 16384
            },
            "cert": {
              "type": "string",
              "maxLength": 16384
            },
            "format": {
              "type": "string",
              "enum": [
                "custom",
                "rfc3164",
                "rfc5424"
              ]
            },
            "key": {
              "type": "string",
              "maxLength": 16384
            },
            "logline": {
              "type": "string",
              "maxLength": 512,
              "minLength": 1,
              "pattern": "^[ -~\\t]+$"
            },
            "max_message_size": {
              "type": "integer",
              "maximum": 2147483647,
              "minimum": 2048
            },
            "port": {
              "type": "integer",
              "maximum": 65535,
              "minimum": 1
            },
            "sd": {
              "type": "string",
              "maxLength": 1024
            },
            "server": {
              "type": "string",
              "maxLength": 255,
              "minLength": 4
            },
            "tls": {
              "type": "boolean"
            }
          },
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    },
    "status": {
      "type": "object",
      "required": [
        "conditions",
        "id"
      ],
      "properties": {
        "conditions": {
          "type": "array",
          "items": {
            "type": "object",
            "required": [
              "lastTransitionTime",
              "message",
              "reason",
              "status",
              "type"
            ],
            "properties": {
              "lastTransitionTime": {
                "type": "string",
                "format": "date-time"
              },
              "message": {
                "type": "string",
                "maxLength": 32768
              },
              "observedGeneration": {
                "type": "integer",
                "format": "int64",
                "minimum": 0
              },
              "reason": {
                "type": "string",
                "maxLength": 1024,
                "minLength": 1,
                "pattern": "^[A-Za-z]([A-Za-z0-9_,:]*[A-Za-z0-9_])?$"
              },
              "status": {
                "type": "string",
                "enum": [
                  "True",
                  "False",
                  "Unknown"
                ]
              },
              "type": {
                "type": "string",
                "maxLength": 316,
                "pattern": "^([a-z0-9]([-a-z0-9]*[a-z0-9])?(\\.[a-z0-9]([-a-z0-9]*[a-z0-9])?)*/)?(([A-Za-z0-9][-A-Za-z0-9_.]*)?[A-Za-z0-9])$"
              }
            },
            "additionalProperties": false
          }
        },
        "id": {
          "type": "string"
        }
      },
      "additionalProperties": false
    }
  }
}
