{
  "properties": {
    "apiVersion": {
      "type": "string"
    },
    "kind": {
      "type": "string"
    },
    "metadata": {
      "type": "object"
    },
    "spec": {
      "additionalProperties": false,
      "properties": {
        "authSecretRef": {
          "additionalProperties": false,
          "properties": {
            "key": {
              "minLength": 1,
              "type": "string"
            },
            "name": {
              "minLength": 1,
              "type": "string"
            }
          },
          "required": [
            "key",
            "name"
          ],
          "type": "object"
        },
        "autoscaler": {
          "additionalProperties": false,
          "properties": {
            "autoscaling": {
              "items": {
                "additionalProperties": false,
                "properties": {
                  "cap_gb": {
                    "maximum": 10000,
                    "minimum": 50,
                    "type": "integer"
                  },
                  "type": {
                    "enum": [
                      "autoscale_disk"
                    ],
                    "type": "string"
                  }
                },
                "required": [
                  "cap_gb",
                  "type"
                ],
                "type": "object"
              },
              "maxItems": 64,
              "type": "array"
            }
          },
          "required": [
            "autoscaling"
          ],
          "type": "object"
        },
        "datadog": {
          "additionalProperties": false,
          "properties": {
            "datadog_api_key": {
              "maxLength": 256,
              "minLength": 1,
              "pattern": "^[A-Za-z0-9]{1,256}$",
              "type": "string"
            },
            "datadog_tags": {
              "items": {
                "additionalProperties": false,
                "properties": {
                  "comment": {
                    "maxLength": 1024,
                    "type": "string"
                  },
                  "tag": {
                    "maxLength": 200,
                    "minLength": 1,
                    "type": "string"
                  }
                },
                "required": [
                  "tag"
                ],
                "type": "object"
              },
              "maxItems": 32,
              "type": "array"
            },
            "disable_consumer_stats": {
              "type": "boolean"
            },
            "extra_tags_prefix": {
              "maxLength": 64,
              "minLength": 0,
              "pattern": "^[A-Za-z0-9\\-]{0,64}$",
              "type": "string"
            },
            "kafka_consumer_check_instances": {
              "maximum": 100,
              "minimum": 1,
              "type": "integer"
            },
            "kafka_consumer_stats_timeout": {
              "maximum": 300,
              "minimum": 2,
              "type": "integer"
            },
            "max_partition_contexts": {
              "maximum": 200000,
              "minimum": 200,
              "type": "integer"
            },
            "site": {
              "enum": [
                "ap1.datadoghq.com",
                "datadoghq.com",
                "datadoghq.eu",
                "ddog-gov.com",
                "us3.datadoghq.com",
                "us5.datadoghq.com"
              ],
              "type": "string"
            }
          },
          "required": [
            "datadog_api_key"
          ],
          "type": "object"
        },
        "endpointName": {
          "maxLength": 36,
          "type": "string",
          "x-kubernetes-validations": [
            {
              "message": "Value is immutable",
              "rule": "self == oldSelf"
            }
          ]
        },
        "endpointType": {
          "enum": [
            "autoscaler",
            "datadog",
            "external_aws_cloudwatch_logs",
            "external_aws_cloudwatch_metrics",
            "external_aws_s3",
            "external_clickhouse",
            "external_elasticsearch_logs",
            "external_google_cloud_bigquery",
            "external_google_cloud_logging",
            "external_kafka",
            "external_mysql",
            "external_opensearch_logs",
            "external_postgresql",
            "external_redis",
            "external_schema_registry",
            "external_sumologic_logs",
            "jolokia",
            "prometheus",
            "rsyslog"
          ],
          "type": "string",
          "x-kubernetes-validations": [
            {
              "message": "Value is immutable",
              "rule": "self == oldSelf"
            }
          ]
        },
        "externalAWSCloudwatchLogs": {
          "additionalProperties": false,
          "properties": {
            "access_key": {
              "maxLength": 4096,
              "type": "string"
            },
            "log_group_name": {
              "maxLength": 512,
              "minLength": 1,
              "pattern": "^[\\.\\-_/#A-Za-z0-9]+$",
              "type": "string"
            },
            "region": {
              "maxLength": 32,
              "type": "string"
            },
            "secret_key": {
              "maxLength": 4096,
              "type": "string"
            }
          },
          "required": [
            "access_key",
            "region",
            "secret_key"
          ],
          "type": "object"
        },
        "externalAWSCloudwatchMetrics": {
          "additionalProperties": false,
          "properties": {
            "access_key": {
              "maxLength": 4096,
              "type": "string"
            },
            "namespace": {
              "maxLength": 255,
              "minLength": 1,
              "type": "string"
            },
            "region": {
              "maxLength": 32,
              "type": "string"
            },
            "secret_key": {
              "maxLength": 4096,
              "type": "string"
            }
          },
          "required": [
            "access_key",
            "namespace",
            "region",
            "secret_key"
          ],
          "type": "object"
        },
        "externalElasticsearchLogs": {
          "additionalProperties": false,
          "properties": {
            "ca": {
              "maxLength": 16384,
              "type": "string"
            },
            "index_days_max": {
              "maximum": 10000,
              "minimum": 1,
              "type": "integer"
            },
            "index_prefix": {
              "maxLength": 1000,
              "minLength": 1,
              "pattern": "^[a-z0-9][a-z0-9-_.]+$",
              "type": "string"
            },
            "timeout": {
              "maximum": 120,
              "minimum": 10,
              "type": "number"
            },
            "url": {
              "maxLength": 2048,
              "minLength": 12,
              "type": "string"
            }
          },
          "required": [
            "index_prefix",
            "url"
          ],
          "type": "object"
        },
        "externalGoogleCloudBigquery": {
          "additionalProperties": false,
          "properties": {
            "project_id": {
              "maxLength": 30,
              "minLength": 6,
              "type": "string"
            },
            "service_account_credentials": {
              "maxLength": 4096,
              "type": "string"
            }
          },
          "required": [
            "project_id",
            "service_account_credentials"
          ],
          "type": "object"
        },
        "externalGoogleCloudLogging": {
          "additionalProperties": false,
          "properties": {
            "log_id": {
              "maxLength": 512,
              "type": "string"
            },
            "project_id": {
              "maxLength": 30,
              "minLength": 6,
              "type": "string"
            },
            "service_account_credentials": {
              "maxLength": 4096,
              "type": "string"
            }
          },
          "required": [
            "log_id",
            "project_id",
            "service_account_credentials"
          ],
          "type": "object"
        },
        "externalKafka": {
          "additionalProperties": false,
          "properties": {
            "bootstrap_servers": {
              "maxLength": 256,
              "minLength": 3,
              "type": "string"
            },
            "sasl_mechanism": {
              "enum": [
                "PLAIN",
                "SCRAM-SHA-256",
                "SCRAM-SHA-512"
              ],
              "type": "string"
            },
            "sasl_plain_password": {
              "maxLength": 256,
              "minLength": 1,
              "type": "string"
            },
            "sasl_plain_username": {
              "maxLength": 256,
              "minLength": 1,
              "type": "string"
            },
            "security_protocol": {
              "enum": [
                "PLAINTEXT",
                "SASL_PLAINTEXT",
                "SASL_SSL",
                "SSL"
              ],
              "type": "string"
            },
            "ssl_ca_cert": {
              "maxLength": 16384,
              "type": "string"
            },
            "ssl_client_cert": {
              "maxLength": 16384,
              "type": "string"
            },
            "ssl_client_key": {
              "maxLength": 16384,
              "type": "string"
            },
            "ssl_endpoint_identification_algorithm": {
              "enum": [
                "https"
              ],
              "type": "string"
            }
          },
          "required": [
            "bootstrap_servers",
            "security_protocol"
          ],
          "type": "object"
        },
        "externalOpensearchLogs": {
          "additionalProperties": false,
          "properties": {
            "ca": {
              "maxLength": 16384,
              "type": "string"
            },
            "index_days_max": {
              "maximum": 10000,
              "minimum": 1,
              "type": "integer"
            },
            "index_prefix": {
              "maxLength": 1000,
              "minLength": 1,
              "pattern": "^[a-z0-9][a-z0-9-_.]+$",
              "type": "string"
            },
            "timeout": {
              "maximum": 120,
              "minimum": 10,
              "type": "number"
            },
            "url": {
              "maxLength": 2048,
              "minLength": 12,
              "type": "string"
            }
          },
          "required": [
            "index_prefix",
            "url"
          ],
          "type": "object"
        },
        "externalPostgresql": {
          "additionalProperties": false,
          "properties": {
            "default_database": {
              "maxLength": 63,
              "pattern": "^[_A-Za-z0-9][-_A-Za-z0-9]{0,62}$",
              "type": "string"
            },
            "host": {
              "maxLength": 255,
              "type": "string"
            },
            "password": {
              "maxLength": 256,
              "type": "string"
            },
            "port": {
              "maximum": 65535,
              "minimum": 1,
              "type": "integer"
            },
            "ssl_client_certificate": {
              "maxLength": 16384,
              "type": "string"
            },
            "ssl_client_key": {
              "maxLength": 16384,
              "type": "string"
            },
            "ssl_mode": {
              "enum": [
                "prefer",
                "require",
                "verify-ca",
                "verify-full"
              ],
              "type": "string"
            },
            "ssl_root_cert": {
              "maxLength": 16384,
              "type": "string"
            },
            "username": {
              "maxLength": 256,
              "type": "string"
            }
          },
          "required": [
            "host",
            "port",
            "username"
          ],
          "type": "object"
        },
        "externalSchemaRegistry": {
          "additionalProperties": false,
          "properties": {
            "authentication": {
              "enum": [
                "basic",
                "none"
              ],
              "type": "string"
            },
            "basic_auth_password": {
              "maxLength": 256,
              "type": "string"
            },
            "basic_auth_username": {
              "maxLength": 256,
              "type": "string"
            },
            "url": {
              "maxLength": 2048,
              "type": "string"
            }
          },
          "required": [
            "authentication",
            "url"
          ],
          "type": "object"
        },
        "jolokia": {
          "additionalProperties": false,
          "properties": {
            "basic_auth_password": {
              "maxLength": 64,
              "minLength": 8,
              "type": "string"
            },
            "basic_auth_username": {
              "maxLength": 32,
              "minLength": 5,
              "pattern": "^[a-z0-9\\-@_]{5,32}$",
              "type": "string"
            }
          },
          "type": "object"
        },
        "project": {
          "maxLength": 63,
          "pattern": "^[a-zA-Z0-9_-]+$",
          "type": "string",
          "x-kubernetes-validations": [
            {
              "message": "Value is immutable",
              "rule": "self == oldSelf"
            }
          ]
        },
        "prometheus": {
          "additionalProperties": false,
          "properties": {
            "basic_auth_password": {
              "maxLength": 64,
              "minLength": 8,
              "type": "string"
            },
            "basic_auth_username": {
              "maxLength": 32,
              "minLength": 5,
              "pattern": "^[a-z0-9\\-@_]{5,32}$",
              "type": "string"
            }
          },
          "type": "object"
        },
        "rsyslog": {
          "additionalProperties": false,
          "properties": {
            "ca": {
              "maxLength": 16384,
              "type": "string"
            },
            "cert": {
              "maxLength": 16384,
              "type": "string"
            },
            "format": {
              "enum": [
                "custom",
                "rfc3164",
                "rfc5424"
              ],
              "type": "string"
            },
            "key": {
              "maxLength": 16384,
              "type": "string"
            },
            "logline": {
              "maxLength": 512,
              "minLength": 1,
              "pattern": "^[ -~\\t]+$",
              "type": "string"
            },
            "max_message_size": {
              "maximum": 2147483647,
              "minimum": 2048,
              "type": "integer"
            },
            "port": {
              "maximum": 65535,
              "minimum": 1,
              "type": "integer"
            },
            "sd": {
              "maxLength": 1024,
              "type": "string"
            },
            "server": {
              "maxLength": 255,
              "minLength": 4,
              "type": "string"
            },
            "tls": {
              "type": "boolean"
            }
          },
          "required": [
            "format",
            "port",
            "server",
            "tls"
          ],
          "type": "object"
        }
      },
      "required": [
        "endpointType",
        "project"
      ],
      "type": "object"
    },
    "status": {
      "additionalProperties": false,
      "properties": {
        "conditions": {
          "items": {
            "additionalProperties": false,
            "properties": {
              "lastTransitionTime": {
                "format": "date-time",
                "type": "string"
              },
              "message": {
                "maxLength": 32768,
                "type": "string"
              },
              "observedGeneration": {
                "format": "int64",
                "minimum": 0,
                "type": "integer"
              },
              "reason": {
                "maxLength": 1024,
                "minLength": 1,
                "pattern": "^[A-Za-z]([A-Za-z0-9_,:]*[A-Za-z0-9_])?$",
                "type": "string"
              },
              "status": {
                "enum": [
                  "True",
                  "False",
                  "Unknown"
                ],
                "type": "string"
              },
              "type": {
                "maxLength": 316,
                "pattern": "^([a-z0-9]([-a-z0-9]*[a-z0-9])?(\\.[a-z0-9]([-a-z0-9]*[a-z0-9])?)*/)?(([A-Za-z0-9][-A-Za-z0-9_.]*)?[A-Za-z0-9])$",
                "type": "string"
              }
            },
            "required": [
              "lastTransitionTime",
              "message",
              "reason",
              "status",
              "type"
            ],
            "type": "object"
          },
          "type": "array"
        },
        "id": {
          "type": "string"
        }
      },
      "required": [
        "conditions",
        "id"
      ],
      "type": "object"
    }
  },
  "type": "object"
}
