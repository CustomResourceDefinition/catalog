{
  "properties": {
    "apiVersion": {
      "type": "string"
    },
    "kind": {
      "type": "string"
    },
    "metadata": {
      "type": "object"
    },
    "spec": {
      "additionalProperties": false,
      "properties": {
        "accountId": {
          "maxLength": 32,
          "type": "string"
        },
        "authSecretRef": {
          "additionalProperties": false,
          "properties": {
            "key": {
              "minLength": 1,
              "type": "string"
            },
            "name": {
              "minLength": 1,
              "type": "string"
            }
          },
          "required": [
            "key",
            "name"
          ],
          "type": "object"
        },
        "billingAddress": {
          "maxLength": 1000,
          "type": "string"
        },
        "billingCurrency": {
          "enum": [
            "AUD",
            "CAD",
            "CHF",
            "DKK",
            "EUR",
            "GBP",
            "NOK",
            "SEK",
            "USD"
          ],
          "type": "string"
        },
        "billingEmails": {
          "items": {
            "type": "string"
          },
          "maxItems": 10,
          "type": "array"
        },
        "billingExtraText": {
          "maxLength": 1000,
          "type": "string"
        },
        "billingGroupId": {
          "maxLength": 36,
          "minLength": 36,
          "type": "string"
        },
        "cardId": {
          "maxLength": 64,
          "type": "string"
        },
        "cloud": {
          "maxLength": 256,
          "type": "string"
        },
        "connInfoSecretTarget": {
          "additionalProperties": false,
          "properties": {
            "name": {
              "type": "string"
            }
          },
          "required": [
            "name"
          ],
          "type": "object"
        },
        "copyFromProject": {
          "maxLength": 63,
          "type": "string"
        },
        "countryCode": {
          "maxLength": 2,
          "minLength": 2,
          "type": "string"
        },
        "tags": {
          "additionalProperties": {
            "type": "string"
          },
          "type": "object"
        },
        "technicalEmails": {
          "items": {
            "type": "string"
          },
          "maxItems": 10,
          "type": "array"
        }
      },
      "type": "object"
    },
    "status": {
      "additionalProperties": false,
      "properties": {
        "availableCredits": {
          "type": "string"
        },
        "conditions": {
          "items": {
            "additionalProperties": false,
            "properties": {
              "lastTransitionTime": {
                "format": "date-time",
                "type": "string"
              },
              "message": {
                "maxLength": 32768,
                "type": "string"
              },
              "observedGeneration": {
                "format": "int64",
                "minimum": 0,
                "type": "integer"
              },
              "reason": {
                "maxLength": 1024,
                "minLength": 1,
                "pattern": "^[A-Za-z]([A-Za-z0-9_,:]*[A-Za-z0-9_])?$",
                "type": "string"
              },
              "status": {
                "enum": [
                  "True",
                  "False",
                  "Unknown"
                ],
                "type": "string"
              },
              "type": {
                "maxLength": 316,
                "pattern": "^([a-z0-9]([-a-z0-9]*[a-z0-9])?(\\.[a-z0-9]([-a-z0-9]*[a-z0-9])?)*/)?(([A-Za-z0-9][-A-Za-z0-9_.]*)?[A-Za-z0-9])$",
                "type": "string"
              }
            },
            "required": [
              "lastTransitionTime",
              "message",
              "reason",
              "status",
              "type"
            ],
            "type": "object"
          },
          "type": "array"
        },
        "country": {
          "type": "string"
        },
        "estimatedBalance": {
          "type": "string"
        },
        "paymentMethod": {
          "type": "string"
        },
        "vatId": {
          "maxLength": 64,
          "type": "string"
        }
      },
      "required": [
        "conditions"
      ],
      "type": "object"
    }
  },
  "type": "object"
}
