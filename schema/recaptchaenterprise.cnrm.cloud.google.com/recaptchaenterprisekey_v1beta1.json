{
  "type": "object",
  "required": [
    "spec"
  ],
  "properties": {
    "apiVersion": {
      "type": "string"
    },
    "kind": {
      "type": "string"
    },
    "metadata": {
      "type": "object"
    },
    "spec": {
      "type": "object",
      "required": [
        "displayName",
        "projectRef"
      ],
      "properties": {
        "androidSettings": {
          "type": "object",
          "properties": {
            "allowAllPackageNames": {
              "type": "boolean"
            },
            "allowedPackageNames": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          },
          "additionalProperties": false
        },
        "displayName": {
          "type": "string"
        },
        "iosSettings": {
          "type": "object",
          "properties": {
            "allowAllBundleIds": {
              "type": "boolean"
            },
            "allowedBundleIds": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          },
          "additionalProperties": false
        },
        "projectRef": {
          "type": "object",
          "oneOf": [
            {
              "required": [
                "name"
              ],
              "not": {
                "required": [
                  "external"
                ]
              }
            },
            {
              "required": [
                "external"
              ],
              "not": {
                "anyOf": [
                  {
                    "required": [
                      "name"
                    ]
                  },
                  {
                    "required": [
                      "namespace"
                    ]
                  }
                ]
              }
            }
          ],
          "properties": {
            "external": {
              "type": "string"
            },
            "name": {
              "type": "string"
            },
            "namespace": {
              "type": "string"
            }
          },
          "additionalProperties": false
        },
        "resourceID": {
          "type": "string"
        },
        "testingOptions": {
          "type": "object",
          "properties": {
            "testingChallenge": {
              "type": "string"
            },
            "testingScore": {
              "type": "number",
              "format": "double"
            }
          },
          "additionalProperties": false
        },
        "webSettings": {
          "type": "object",
          "required": [
            "integrationType"
          ],
          "properties": {
            "allowAllDomains": {
              "type": "boolean"
            },
            "allowAmpTraffic": {
              "type": "boolean"
            },
            "allowedDomains": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "challengeSecurityPreference": {
              "type": "string"
            },
            "integrationType": {
              "type": "string"
            }
          },
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    },
    "status": {
      "type": "object",
      "properties": {
        "conditions": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "lastTransitionTime": {
                "type": "string"
              },
              "message": {
                "type": "string"
              },
              "reason": {
                "type": "string"
              },
              "status": {
                "type": "string"
              },
              "type": {
                "type": "string"
              }
            },
            "additionalProperties": false
          }
        },
        "createTime": {
          "type": "string",
          "format": "date-time"
        },
        "observedGeneration": {
          "type": "integer"
        }
      },
      "additionalProperties": false
    }
  }
}
