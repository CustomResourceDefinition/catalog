{
  "type": "object",
  "properties": {
    "apiVersion": {
      "type": "string"
    },
    "kind": {
      "type": "string"
    },
    "metadata": {
      "type": "object"
    },
    "spec": {
      "type": "object",
      "properties": {
        "controlPlaneEndpoint": {
          "type": "object",
          "required": [
            "host",
            "port"
          ],
          "properties": {
            "host": {
              "type": "string"
            },
            "port": {
              "type": "integer",
              "format": "int32"
            }
          },
          "additionalProperties": false
        },
        "imageLookupBaseRegistry": {
          "type": "string",
          "default": "ghcr.io/tinkerbell/cluster-api-provider-tinkerbell"
        },
        "imageLookupFormat": {
          "type": "string"
        },
        "imageLookupOSDistro": {
          "type": "string",
          "default": "ubuntu"
        },
        "imageLookupOSVersion": {
          "type": "string"
        }
      },
      "additionalProperties": false
    },
    "status": {
      "type": "object",
      "properties": {
        "ready": {
          "type": "boolean"
        }
      },
      "additionalProperties": false
    }
  }
}
