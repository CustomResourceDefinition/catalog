              },
              "message": {
                "type": "string"
              },
              "reason": {
                "type": "string"
              },
              "severity": {
                "type": "string"
              },
              "status": {
                "type": "string"
              },
              "type": {
                "type": "string"
              }
            },
            "additionalProperties": false
          }
        },
