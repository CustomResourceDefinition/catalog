                    "idleTimeout": {
                      "type": "integer",
                      "format": "int64"
                    }
                  },
                  "additionalProperties": false
                },
                "availabilityZones": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  }
                },
                "dnsName": {
                  "type": "string"
                },
