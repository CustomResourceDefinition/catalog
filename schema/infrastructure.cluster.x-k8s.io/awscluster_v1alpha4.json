            "id"
          ],
          "properties": {
            "addresses": {
              "type": "array",
              "items": {
                "type": "object",
                "required": [
                  "address",
                  "type"
                ],
                "properties": {
                  "address": {
                    "type": "string"
                  },
                  "type": {
                    "type": "string"
                  }
                },
                "additionalProperties": false
              }
            },
            "availabilityZone": {
              "type": "string"
            },
            "ebsOptimized": {
              "type": "boolean"
            },
            "enaSupport": {
              "type": "boolean"
