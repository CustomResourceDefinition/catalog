                  "encrypted": {
                    "type": "boolean"
                  },
                  "encryptionKey": {
                    "type": "string"
                  },
                  "iops": {
                    "type": "integer",
                    "format": "int64"
                  },
                  "size": {
                    "type": "integer",
                    "format": "int64",
                    "minimum": 8
                  },
                  "throughput": {
                    "type": "integer",
                    "format": "int64"
                  },
                  "type": {
                    "type": "string"
                  }
