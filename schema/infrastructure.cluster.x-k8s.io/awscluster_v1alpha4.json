                  "isPublic": {
                    "type": "boolean"
                  },
                  "natGatewayId": {
                    "type": "string"
                  },
                  "routeTableId": {
                    "type": "string"
                  },
                  "tags": {
                    "type": "object",
                    "additionalProperties": {
                      "type": "string"
                    }
                  }
                },
                "additionalProperties": false
              }
            },
            "vpc": {
              "type": "object",
              "properties": {
                "availabilityZoneSelection": {
                  "type": "string",
                  "default": "Ordered",
                  "enum": [
                    "Ordered",
                    "Random"
                  ]
                },
                "availabilityZoneUsageLimit": {
                  "type": "integer",
                  "default": 3,
                  "minimum": 1
                },
                "cidrBlock": {
                  "type": "string"
                },
                "id": {
                  "type": "string"
                },
                "internetGatewayId": {
                  "type": "string"
                },
                "tags": {
                  "type": "object",
                  "additionalProperties": {
                    "type": "string"
                  }
                }
              },
              "additionalProperties": false
            }
          },
          "additionalProperties": false
        },
        "region": {
          "type": "string"
        },
        "sshKeyName": {
          "type": "string"
        }
      },
      "additionalProperties": false
