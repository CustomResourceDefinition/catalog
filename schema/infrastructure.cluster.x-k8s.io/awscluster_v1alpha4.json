                }
              },
              "additionalProperties": false
            },
            "securityGroupOverrides": {
              "type": "object",
              "additionalProperties": {
                "type": "string"
              }
            },
            "subnets": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "availabilityZone": {
                    "type": "string"
                  },
                  "cidrBlock": {
                    "type": "string"
                  },
                  "id": {
                    "type": "string"
