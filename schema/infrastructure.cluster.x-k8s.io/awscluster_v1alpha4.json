          "properties": {
            "kind": {
              "type": "string",
              "enum": [
                "AWSClusterControllerIdentity",
                "AWSClusterRoleIdentity",
                "AWSClusterStaticIdentity"
              ]
            },
            "name": {
              "type": "string",
              "minLength": 1
            }
          },
          "additionalProperties": false
        },
        "imageLookupBaseOS": {
          "type": "string"
        },
        "imageLookupFormat": {
          "type": "string"
