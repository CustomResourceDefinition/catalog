                "encrypted": {
                  "type": "boolean"
                },
                "encryptionKey": {
                  "type": "string"
                },
                "iops": {
                  "type": "integer",
                  "format": "int64"
                },
                "size": {
                  "type": "integer",
                  "format": "int64",
                  "minimum": 8
                },
                "throughput": {
                  "type": "integer",
                  "format": "int64"
                },
                "type": {
                  "type": "string"
                }
              },
              "additionalProperties": false
            },
            "securityGroupIds": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "spotMarketOptions": {
              "type": "object",
              "properties": {
                "maxPrice": {
                  "type": "string"
                }
              },
              "additionalProperties": false
            },
            "sshKeyName": {
              "type": "string"
            },
            "subnetId": {
              "type": "string"
            },
            "tags": {
              "type": "object",
              "additionalProperties": {
                "type": "string"
              }
            },
            "tenancy": {
              "type": "string"
            },
            "type": {
              "type": "string"
            },
            "userData": {
              "type": "string"
            },
            "volumeIDs": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          },
          "additionalProperties": false
