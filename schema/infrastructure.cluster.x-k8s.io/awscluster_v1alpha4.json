            "ami": {
              "type": "string"
            },
            "disableIngressRules": {
              "type": "boolean"
            },
            "enabled": {
              "type": "boolean"
            },
            "instanceType": {
              "type": "string"
            }
          },
          "additionalProperties": false
        },
        "controlPlaneEndpoint": {
          "type": "object",
          "required": [
            "host",
            "port"
          ],
          "properties": {
            "host": {
              "type": "string"
            },
            "port": {
              "type": "integer",
              "format": "int32"
            }
          },
          "additionalProperties": false
        },
        "controlPlaneLoadBalancer": {
          "type": "object",
          "properties": {
            "additionalSecurityGroups": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "crossZoneLoadBalancing": {
              "type": "boolean"
            },
            "scheme": {
              "type": "string",
              "default": "internet-facing",
              "enum": [
                "internet-facing",
                "Internet-facing",
                "internal"
              ]
            },
            "subnets": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          },
          "additionalProperties": false
