{
  "type": "object",
  "properties": {
    "apiVersion": {
      "type": "string"
    },
    "kind": {
      "type": "string"
    },
    "metadata": {
      "type": "object"
    },
    "spec": {
      "type": "object",
      "required": [
        "template"
      ],
      "properties": {
        "template": {
          "type": "object",
          "required": [
            "spec"
          ],
          "properties": {
            "spec": {
              "type": "object",
              "properties": {
                "additionalTags": {
                  "type": "object",
                  "additionalProperties": {
                    "type": "string"
                  }
                },
                "bastion": {
                  "type": "object",
                  "properties": {
                    "allowedCIDRBlocks": {
                      "type": "array",
                      "items": {
                        "type": "string"
                      }
                    },
                    "ami": {
                      "type": "string"
                    },
                    "disableIngressRules": {
                      "type": "boolean"
                    },
                    "enabled": {
                      "type": "boolean"
                    },
                    "instanceType": {
                      "type": "string"
                    }
                  },
                  "additionalProperties": false
                },
                "controlPlaneEndpoint": {
                  "type": "object",
                  "required": [
                    "host",
                    "port"
                  ],
                  "properties": {
                    "host": {
                      "type": "string"
                    },
                    "port": {
                      "type": "integer",
                      "format": "int32"
                    }
                  },
                  "additionalProperties": false
                },
                "controlPlaneLoadBalancer": {
                  "type": "object",
                  "properties": {
                    "additionalSecurityGroups": {
                      "type": "array",
                      "items": {
                        "type": "string"
                      }
                    },
                    "crossZoneLoadBalancing": {
                      "type": "boolean"
                    },
                    "scheme": {
                      "type": "string",
                      "default": "internet-facing",
                      "enum": [
                        "internet-facing",
                        "Internet-facing",
                        "internal"
                      ]
                    },
                    "subnets": {
                      "type": "array",
                      "items": {
                        "type": "string"
                      }
                    }
                  },
                  "additionalProperties": false
                },
                "identityRef": {
                  "type": "object",
                  "required": [
                    "kind",
                    "name"
                  ],
                  "properties": {
                    "kind": {
                      "type": "string",
                      "enum": [
                        "AWSClusterControllerIdentity",
                        "AWSClusterRoleIdentity",
                        "AWSClusterStaticIdentity"
                      ]
                    },
                    "name": {
                      "type": "string",
                      "minLength": 1
                    }
                  },
                  "additionalProperties": false
                },
                "imageLookupBaseOS": {
                  "type": "string"
                },
                "imageLookupFormat": {
                  "type": "string"
                },
                "imageLookupOrg": {
                  "type": "string"
                },
                "network": {
                  "type": "object",
                  "properties": {
                    "cni": {
                      "type": "object",
                      "properties": {
                        "cniIngressRules": {
                          "type": "array",
                          "items": {
                            "type": "object",
                            "required": [
                              "description",
                              "fromPort",
                              "protocol",
                              "toPort"
                            ],
                            "properties": {
                              "description": {
                                "type": "string"
                              },
                              "fromPort": {
                                "type": "integer",
                                "format": "int64"
                              },
                              "protocol": {
                                "type": "string"
                              },
                              "toPort": {
                                "type": "integer",
                                "format": "int64"
                              }
                            },
                            "additionalProperties": false
                          }
                        }
                      },
                      "additionalProperties": false
                    },
                    "securityGroupOverrides": {
                      "type": "object",
                      "additionalProperties": {
                        "type": "string"
                      }
                    },
                    "subnets": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "availabilityZone": {
                            "type": "string"
                          },
                          "cidrBlock": {
                            "type": "string"
                          },
                          "id": {
                            "type": "string"
                          },
                          "isPublic": {
                            "type": "boolean"
                          },
                          "natGatewayId": {
                            "type": "string"
                          },
                          "routeTableId": {
                            "type": "string"
                          },
                          "tags": {
                            "type": "object",
                            "additionalProperties": {
                              "type": "string"
                            }
                          }
                        },
                        "additionalProperties": false
                      }
                    },
                    "vpc": {
                      "type": "object",
                      "properties": {
                        "availabilityZoneSelection": {
                          "type": "string",
                          "default": "Ordered",
                          "enum": [
                            "Ordered",
                            "Random"
                          ]
                        },
                        "availabilityZoneUsageLimit": {
                          "type": "integer",
                          "default": 3,
                          "minimum": 1
                        },
                        "cidrBlock": {
                          "type": "string"
                        },
                        "id": {
                          "type": "string"
                        },
                        "internetGatewayId": {
                          "type": "string"
                        },
                        "tags": {
                          "type": "object",
                          "additionalProperties": {
                            "type": "string"
                          }
                        }
                      },
                      "additionalProperties": false
                    }
                  },
                  "additionalProperties": false
                },
                "region": {
                  "type": "string"
                },
                "sshKeyName": {
                  "type": "string"
                }
              },
              "additionalProperties": false
            }
          },
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    }
  }
}
