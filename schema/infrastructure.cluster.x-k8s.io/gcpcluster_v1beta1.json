{
  "type": "object",
  "properties": {
    "apiVersion": {
      "type": "string"
    },
    "kind": {
      "type": "string"
    },
    "metadata": {
      "type": "object"
    },
    "spec": {
      "type": "object",
      "required": [
        "project",
        "region"
      ],
      "properties": {
        "additionalLabels": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          }
        },
        "controlPlaneEndpoint": {
          "type": "object",
          "required": [
            "host",
            "port"
          ],
          "properties": {
            "host": {
              "type": "string",
              "maxLength": 512
            },
            "port": {
              "type": "integer",
              "format": "int32"
            }
          },
          "additionalProperties": false
        },
        "credentialsRef": {
          "type": "object",
          "required": [
            "name",
            "namespace"
          ],
          "properties": {
            "name": {
              "type": "string"
            },
            "namespace": {
              "type": "string"
            }
          },
          "additionalProperties": false
        },
        "failureDomains": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "loadBalancer": {
          "type": "object",
          "properties": {
            "apiServerInstanceGroupTagOverride": {
              "type": "string",
              "maxLength": 16,
              "pattern": "(^[1-9][0-9]{0,31}$)|(^[a-z][a-z0-9-]{4,28}[a-z0-9]$)"
            },
            "internalLoadBalancer": {
              "type": "object",
              "properties": {
                "internalAccess": {
                  "type": "string",
                  "default": "Regional",
                  "enum": [
                    "Regional",
                    "Global"
                  ]
                },
                "name": {
                  "type": "string",
                  "pattern": "(^[1-9][0-9]{0,31}$)|(^[a-z][a-z0-9-]{4,28}[a-z0-9]$)"
                },
                "subnet": {
                  "type": "string"
                }
              },
              "additionalProperties": false
            },
            "loadBalancerType": {
              "type": "string"
            }
          },
          "additionalProperties": false
        },
        "network": {
          "type": "object",
          "properties": {
            "autoCreateSubnetworks": {
              "type": "boolean"
            },
            "hostProject": {
              "type": "string"
            },
            "loadBalancerBackendPort": {
              "type": "integer",
              "format": "int32"
            },
            "mtu": {
              "type": "integer",
              "format": "int64",
              "default": 1460,
              "maximum": 8896,
              "minimum": 1300
            },
            "name": {
              "type": "string"
            },
            "subnets": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "cidrBlock": {
                    "type": "string"
                  },
                  "description": {
                    "type": "string"
                  },
                  "enableFlowLogs": {
                    "type": "boolean"
                  },
                  "name": {
                    "type": "string"
                  },
                  "privateGoogleAccess": {
                    "type": "boolean"
                  },
                  "purpose": {
                    "type": "string",
                    "default": "PRIVATE_RFC_1918",
                    "enum": [
                      "INTERNAL_HTTPS_LOAD_BALANCER",
                      "PRIVATE_RFC_1918",
                      "PRIVATE",
                      "PRIVATE_SERVICE_CONNECT",
                      "REGIONAL_MANAGED_PROXY"
                    ]
                  },
                  "region": {
                    "type": "string"
                  },
                  "secondaryCidrBlocks": {
                    "type": "object",
                    "additionalProperties": {
                      "type": "string"
                    }
                  },
                  "stackType": {
                    "type": "string",
                    "default": "IPV4_ONLY",
                    "enum": [
                      "IPV4_ONLY",
                      "IPV4_IPV6",
                      "IPV6_ONLY"
                    ]
                  }
                },
                "additionalProperties": false
              }
            }
          },
          "additionalProperties": false
        },
        "project": {
          "type": "string"
        },
        "region": {
          "type": "string"
        },
        "resourceManagerTags": {
          "type": "array",
          "items": {
            "type": "object",
            "required": [
              "key",
              "parentID",
              "value"
            ],
            "properties": {
              "key": {
                "type": "string",
                "maxLength": 63,
                "minLength": 1,
                "pattern": "^[a-zA-Z0-9]([0-9A-Za-z_.-]{0,61}[a-zA-Z0-9])?$"
              },
              "parentID": {
                "type": "string",
                "maxLength": 32,
                "minLength": 1,
                "pattern": "(^[1-9][0-9]{0,31}$)|(^[a-z][a-z0-9-]{4,28}[a-z0-9]$)"
              },
              "value": {
                "type": "string",
                "maxLength": 63,
                "minLength": 1,
                "pattern": "^[a-zA-Z0-9]([0-9A-Za-z_.@%=+:,*#\u0026()\\[\\]{}\\-\\s]{0,61}[a-zA-Z0-9])?$"
              }
            },
            "additionalProperties": false
          }
        },
        "serviceEndpoints": {
          "type": "object",
          "properties": {
            "compute": {
              "type": "string",
              "format": "uri",
              "pattern": "^https://"
            },
            "container": {
              "type": "string",
              "format": "uri",
              "pattern": "^https://"
            },
            "iam": {
              "type": "string",
              "format": "uri",
              "pattern": "^https://"
            },
            "resourceManager": {
              "type": "string",
              "format": "uri",
              "pattern": "^https://"
            }
          },
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    },
    "status": {
      "type": "object",
      "required": [
        "ready"
      ],
      "properties": {
        "failureDomains": {
          "type": "object",
          "additionalProperties": {
            "description": "FailureDomainSpec is the Schema for Cluster API failure domains.\nIt allows controllers to understand how many failure domains a cluster can optionally span across.",
            "type": "object",
            "properties": {
              "attributes": {
                "description": "attributes is a free form map of attributes an infrastructure provider might use or require.",
                "type": "object",
                "additionalProperties": {
                  "type": "string"
                }
              },
              "controlPlane": {
                "description": "controlPlane determines if this failure domain is suitable for use by control plane machines.",
                "type": "boolean"
              }
            }
          }
        },
        "network": {
          "type": "object",
          "properties": {
            "apiInternalBackendService": {
              "type": "string"
            },
            "apiInternalForwardingRule": {
              "type": "string"
            },
            "apiInternalHealthCheck": {
              "type": "string"
            },
            "apiInternalIpAddress": {
              "type": "string"
            },
            "apiServerBackendService": {
              "type": "string"
            },
            "apiServerForwardingRule": {
              "type": "string"
            },
            "apiServerHealthCheck": {
              "type": "string"
            },
            "apiServerInstanceGroups": {
              "type": "object",
              "additionalProperties": {
                "type": "string"
              }
            },
            "apiServerIpAddress": {
              "type": "string"
            },
            "apiServerTargetProxy": {
              "type": "string"
            },
            "firewallRules": {
              "type": "object",
              "additionalProperties": {
                "type": "string"
              }
            },
            "router": {
              "type": "string"
            },
            "selfLink": {
              "type": "string"
            }
          },
          "additionalProperties": false
        },
        "ready": {
          "type": "boolean"
        }
      },
      "additionalProperties": false
    }
  }
}
