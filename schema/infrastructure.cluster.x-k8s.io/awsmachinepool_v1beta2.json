{
  "type": "object",
  "properties": {
    "apiVersion": {
      "type": "string"
    },
    "kind": {
      "type": "string"
    },
    "metadata": {
      "type": "object"
    },
    "spec": {
      "type": "object",
      "required": [
        "awsLaunchTemplate",
        "maxSize",
        "minSize"
      ],
      "properties": {
        "additionalTags": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          }
        },
        "availabilityZoneSubnetType": {
          "type": "string",
          "enum": [
            "public",
            "private",
            "all"
          ]
        },
        "availabilityZones": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "awsLaunchTemplate": {
          "type": "object",
          "properties": {
            "additionalSecurityGroups": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "filters": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "required": [
                        "name",
                        "values"
                      ],
                      "properties": {
                        "name": {
                          "type": "string"
                        },
                        "values": {
                          "type": "array",
                          "items": {
                            "type": "string"
                          }
                        }
                      },
                      "additionalProperties": false
                    }
                  },
                  "id": {
                    "type": "string"
                  }
                },
                "additionalProperties": false
              }
            },
            "ami": {
              "type": "object",
              "properties": {
                "eksLookupType": {
                  "type": "string",
                  "enum": [
                    "AmazonLinux",
                    "AmazonLinuxGPU",
                    "AmazonLinux2023",
                    "AmazonLinux2023GPU"
                  ]
                },
                "id": {
                  "type": "string"
                }
              },
              "additionalProperties": false
            },
            "capacityReservationId": {
              "type": "string"
            },
            "capacityReservationPreference": {
              "type": "string",
              "allOf": [
                {
                  "enum": [
                    "",
                    "None",
                    "CapacityReservationsOnly",
                    "Open"
                  ]
                },
                {
                  "enum": [
                    "",
                    "None",
                    "CapacityReservationsOnly",
                    "Open"
                  ]
                }
              ]
            },
            "iamInstanceProfile": {
              "type": "string"
            },
            "imageLookupBaseOS": {
              "type": "string"
            },
            "imageLookupFormat": {
              "type": "string"
            },
            "imageLookupOrg": {
              "type": "string"
            },
            "instanceMetadataOptions": {
              "type": "object",
              "properties": {
                "httpEndpoint": {
                  "type": "string",
                  "default": "enabled",
                  "enum": [
                    "enabled",
                    "disabled"
                  ]
                },
                "httpPutResponseHopLimit": {
                  "type": "integer",
                  "format": "int64",
                  "default": 1,
                  "maximum": 64,
                  "minimum": 1
                },
                "httpTokens": {
                  "type": "string",
                  "default": "optional",
                  "enum": [
                    "optional",
                    "required"
                  ]
                },
                "instanceMetadataTags": {
                  "type": "string",
                  "default": "disabled",
                  "enum": [
                    "enabled",
                    "disabled"
                  ]
                }
              },
              "additionalProperties": false
            },
            "instanceType": {
              "type": "string"
            },
            "marketType": {
              "type": "string",
              "enum": [
                "OnDemand",
                "Spot",
                "CapacityBlock"
              ]
            },
            "name": {
              "type": "string"
            },
            "nonRootVolumes": {
              "type": "array",
              "items": {
                "type": "object",
                "required": [
                  "size"
                ],
                "properties": {
                  "deviceName": {
                    "type": "string"
                  },
                  "encrypted": {
                    "type": "boolean"
                  },
                  "encryptionKey": {
                    "type": "string"
                  },
                  "iops": {
                    "type": "integer",
                    "format": "int64"
                  },
                  "size": {
                    "type": "integer",
                    "format": "int64",
                    "minimum": 8
                  },
                  "throughput": {
                    "type": "integer",
                    "format": "int64"
                  },
                  "type": {
                    "type": "string"
                  }
                },
                "additionalProperties": false
              }
            },
            "privateDnsName": {
              "type": "object",
              "properties": {
                "enableResourceNameDnsAAAARecord": {
                  "type": "boolean"
                },
                "enableResourceNameDnsARecord": {
                  "type": "boolean"
                },
                "hostnameType": {
                  "type": "string",
                  "enum": [
                    "ip-name",
                    "resource-name"
                  ]
                }
              },
              "additionalProperties": false
            },
            "rootVolume": {
              "type": "object",
              "required": [
                "size"
              ],
              "properties": {
                "deviceName": {
                  "type": "string"
                },
                "encrypted": {
                  "type": "boolean"
                },
                "encryptionKey": {
                  "type": "string"
                },
                "iops": {
                  "type": "integer",
                  "format": "int64"
                },
                "size": {
                  "type": "integer",
                  "format": "int64",
                  "minimum": 8
                },
                "throughput": {
                  "type": "integer",
                  "format": "int64"
                },
                "type": {
                  "type": "string"
                }
              },
              "additionalProperties": false
            },
            "spotMarketOptions": {
              "type": "object",
              "properties": {
                "maxPrice": {
                  "type": "string"
                }
              },
              "additionalProperties": false
            },
            "sshKeyName": {
              "type": "string"
            },
            "versionNumber": {
              "type": "integer",
              "format": "int64"
            }
          },
          "additionalProperties": false
        },
        "capacityRebalance": {
          "type": "boolean"
        },
        "defaultCoolDown": {
          "type": "string"
        },
        "defaultInstanceWarmup": {
          "type": "string"
        },
        "ignition": {
          "type": "object",
          "properties": {
            "proxy": {
              "type": "object",
              "properties": {
                "httpProxy": {
                  "type": "string"
                },
                "httpsProxy": {
                  "type": "string"
                },
                "noProxy": {
                  "type": "array",
                  "maxItems": 64,
                  "items": {
                    "type": "string",
                    "maxLength": 2048
                  }
                }
              },
              "additionalProperties": false
            },
            "storageType": {
              "type": "string",
              "default": "ClusterObjectStore",
              "enum": [
                "ClusterObjectStore",
                "UnencryptedUserData"
              ]
            },
            "tls": {
              "type": "object",
              "properties": {
                "certificateAuthorities": {
                  "type": "array",
                  "maxItems": 64,
                  "items": {
                    "type": "string",
                    "maxLength": 65536
                  }
                }
              },
              "additionalProperties": false
            },
            "version": {
              "type": "string",
              "enum": [
                "2.3",
                "3.0",
                "3.1",
                "3.2",
                "3.3",
                "3.4"
              ]
            }
          },
          "additionalProperties": false
        },
        "lifecycleHooks": {
          "type": "array",
          "items": {
            "type": "object",
            "required": [
              "lifecycleTransition",
              "name"
            ],
            "properties": {
              "defaultResult": {
                "type": "string",
                "enum": [
                  "CONTINUE",
                  "ABANDON"
                ]
              },
              "heartbeatTimeout": {
                "type": "string",
                "format": "duration"
              },
              "lifecycleTransition": {
                "type": "string",
                "enum": [
                  "autoscaling:EC2_INSTANCE_LAUNCHING",
                  "autoscaling:EC2_INSTANCE_TERMINATING"
                ]
              },
              "name": {
                "type": "string"
              },
              "notificationMetadata": {
                "type": "string"
              },
              "notificationTargetARN": {
                "type": "string"
              },
              "roleARN": {
                "type": "string"
              }
            },
            "additionalProperties": false
          }
        },
        "maxSize": {
          "type": "integer",
          "format": "int32",
          "default": 1,
          "minimum": 1
        },
        "minSize": {
          "type": "integer",
          "format": "int32",
          "default": 1,
          "minimum": 0
        },
        "mixedInstancesPolicy": {
          "type": "object",
          "properties": {
            "instancesDistribution": {
              "type": "object",
              "properties": {
                "onDemandAllocationStrategy": {
                  "type": "string",
                  "default": "prioritized",
                  "enum": [
                    "prioritized",
                    "lowest-price"
                  ]
                },
                "onDemandBaseCapacity": {
                  "type": "integer",
                  "format": "int64",
                  "default": 0
                },
                "onDemandPercentageAboveBaseCapacity": {
                  "type": "integer",
                  "format": "int64",
                  "default": 100
                },
                "spotAllocationStrategy": {
                  "type": "string",
                  "default": "lowest-price",
                  "enum": [
                    "lowest-price",
                    "capacity-optimized",
                    "capacity-optimized-prioritized",
                    "price-capacity-optimized"
                  ]
                }
              },
              "additionalProperties": false
            },
            "overrides": {
              "type": "array",
              "items": {
                "type": "object",
                "required": [
                  "instanceType"
                ],
                "properties": {
                  "instanceType": {
                    "type": "string"
                  }
                },
                "additionalProperties": false
              }
            }
          },
          "additionalProperties": false
        },
        "providerID": {
          "type": "string"
        },
        "providerIDList": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "refreshPreferences": {
          "type": "object",
          "properties": {
            "disable": {
              "type": "boolean"
            },
            "instanceWarmup": {
              "type": "integer",
              "format": "int64"
            },
            "maxHealthyPercentage": {
              "type": "integer",
              "format": "int64",
              "maximum": 200,
              "minimum": 100
            },
            "minHealthyPercentage": {
              "type": "integer",
              "format": "int64"
            },
            "strategy": {
              "type": "string"
            }
          },
          "additionalProperties": false
        },
        "subnets": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "filters": {
                "type": "array",
                "items": {
                  "type": "object",
                  "required": [
                    "name",
                    "values"
                  ],
                  "properties": {
                    "name": {
                      "type": "string"
                    },
                    "values": {
                      "type": "array",
                      "items": {
                        "type": "string"
                      }
                    }
                  },
                  "additionalProperties": false
                }
              },
              "id": {
                "type": "string"
              }
            },
            "additionalProperties": false
          }
        },
        "suspendProcesses": {
          "type": "object",
          "properties": {
            "all": {
              "type": "boolean"
            },
            "processes": {
              "type": "object",
              "properties": {
                "addToLoadBalancer": {
                  "type": "boolean"
                },
                "alarmNotification": {
                  "type": "boolean"
                },
                "azRebalance": {
                  "type": "boolean"
                },
                "healthCheck": {
                  "type": "boolean"
                },
                "instanceRefresh": {
                  "type": "boolean"
                },
                "launch": {
                  "type": "boolean"
                },
                "replaceUnhealthy": {
                  "type": "boolean"
                },
                "scheduledActions": {
                  "type": "boolean"
                },
                "terminate": {
                  "type": "boolean"
                }
              },
              "additionalProperties": false
            }
          },
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    },
    "status": {
      "type": "object",
      "properties": {
        "asgStatus": {
          "type": "string"
        },
        "conditions": {
          "type": "array",
          "items": {
            "type": "object",
            "required": [
              "lastTransitionTime",
              "status",
              "type"
            ],
            "properties": {
              "lastTransitionTime": {
                "type": "string",
                "format": "date-time"
              },
              "message": {
                "type": "string",
                "maxLength": 10240,
                "minLength": 1
              },
              "reason": {
                "type": "string",
                "maxLength": 256,
                "minLength": 1
              },
              "severity": {
                "type": "string",
                "maxLength": 32
              },
              "status": {
                "type": "string"
              },
              "type": {
                "type": "string",
                "maxLength": 256,
                "minLength": 1
              }
            },
            "additionalProperties": false
          }
        },
        "failureMessage": {
          "type": "string"
        },
        "failureReason": {
          "type": "string"
        },
        "infrastructureMachineKind": {
          "type": "string"
        },
        "instances": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "instanceID": {
                "type": "string"
              },
              "version": {
                "type": "string"
              }
            },
            "additionalProperties": false
          }
        },
        "launchTemplateID": {
          "type": "string"
        },
        "launchTemplateVersion": {
          "type": "string"
        },
        "ready": {
          "type": "boolean"
        },
        "replicas": {
          "type": "integer",
          "format": "int32"
        }
      },
      "additionalProperties": false
    }
  }
}
