{
  "type": "object",
  "properties": {
    "apiVersion": {
      "type": "string"
    },
    "kind": {
      "type": "string"
    },
    "metadata": {
      "type": "object"
    },
    "spec": {
      "type": "object",
      "required": [
        "awsLaunchTemplate",
        "maxSize",
        "minSize"
      ],
      "properties": {
        "additionalTags": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          }
        },
        "availabilityZones": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "awsLaunchTemplate": {
          "type": "object",
          "properties": {
            "additionalSecurityGroups": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "arn": {
                    "type": "string"
                  },
                  "filters": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "required": [
                        "name",
                        "values"
                      ],
                      "properties": {
                        "name": {
                          "type": "string"
                        },
                        "values": {
                          "type": "array",
                          "items": {
                            "type": "string"
                          }
                        }
                      },
                      "additionalProperties": false
                    }
                  },
                  "id": {
                    "type": "string"
                  }
                },
                "additionalProperties": false
              }
            },
            "ami": {
              "type": "object",
              "properties": {
                "eksLookupType": {
                  "type": "string",
                  "enum": [
                    "AmazonLinux",
                    "AmazonLinuxGPU"
                  ]
                },
                "id": {
                  "type": "string"
                }
              },
              "additionalProperties": false
            },
            "iamInstanceProfile": {
              "type": "string"
            },
            "imageLookupBaseOS": {
              "type": "string"
            },
            "imageLookupFormat": {
              "type": "string"
            },
            "imageLookupOrg": {
              "type": "string"
            },
            "instanceType": {
              "type": "string"
            },
            "name": {
              "type": "string"
            },
            "rootVolume": {
              "type": "object",
              "required": [
                "size"
              ],
              "properties": {
                "deviceName": {
                  "type": "string"
                },
                "encrypted": {
                  "type": "boolean"
                },
                "encryptionKey": {
                  "type": "string"
                },
                "iops": {
                  "type": "integer",
                  "format": "int64"
                },
                "size": {
                  "type": "integer",
                  "format": "int64",
                  "minimum": 8
                },
                "throughput": {
                  "type": "integer",
                  "format": "int64"
                },
                "type": {
                  "type": "string"
                }
              },
              "additionalProperties": false
            },
            "sshKeyName": {
              "type": "string"
            },
            "versionNumber": {
              "type": "integer",
              "format": "int64"
            }
          },
          "additionalProperties": false
        },
        "capacityRebalance": {
          "type": "boolean"
        },
        "defaultCoolDown": {
          "type": "string"
        },
        "maxSize": {
          "type": "integer",
          "format": "int32",
          "default": 1,
          "minimum": 1
        },
        "minSize": {
          "type": "integer",
          "format": "int32",
          "default": 1,
          "minimum": 1
        },
        "mixedInstancesPolicy": {
          "type": "object",
          "properties": {
            "instancesDistribution": {
              "type": "object",
              "properties": {
                "onDemandAllocationStrategy": {
                  "type": "string",
                  "default": "prioritized",
                  "enum": [
                    "prioritized"
                  ]
                },
                "onDemandBaseCapacity": {
                  "type": "integer",
                  "format": "int64",
                  "default": 0
                },
                "onDemandPercentageAboveBaseCapacity": {
                  "type": "integer",
                  "format": "int64",
                  "default": 100
                },
                "spotAllocationStrategy": {
                  "type": "string",
                  "default": "lowest-price",
                  "enum": [
                    "lowest-price",
                    "capacity-optimized"
                  ]
                }
              },
              "additionalProperties": false
            },
            "overrides": {
              "type": "array",
              "items": {
                "type": "object",
                "required": [
                  "instanceType"
                ],
                "properties": {
                  "instanceType": {
                    "type": "string"
                  }
                },
                "additionalProperties": false
              }
            }
          },
          "additionalProperties": false
        },
        "providerID": {
          "type": "string"
        },
        "providerIDList": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "refreshPreferences": {
          "type": "object",
          "properties": {
            "instanceWarmup": {
              "type": "integer",
              "format": "int64"
            },
            "minHealthyPercentage": {
              "type": "integer",
              "format": "int64"
            },
            "strategy": {
              "type": "string"
            }
          },
          "additionalProperties": false
        },
        "subnets": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "arn": {
                "type": "string"
              },
              "filters": {
                "type": "array",
                "items": {
                  "type": "object",
                  "required": [
                    "name",
                    "values"
                  ],
                  "properties": {
                    "name": {
                      "type": "string"
                    },
                    "values": {
                      "type": "array",
                      "items": {
                        "type": "string"
                      }
                    }
                  },
                  "additionalProperties": false
                }
              },
              "id": {
                "type": "string"
              }
            },
            "additionalProperties": false
          }
        }
      },
      "additionalProperties": false
    },
    "status": {
      "type": "object",
      "properties": {
        "asgStatus": {
          "type": "string"
        },
        "conditions": {
          "type": "array",
          "items": {
            "type": "object",
            "required": [
              "status",
              "type"
            ],
            "properties": {
              "lastTransitionTime": {
                "type": "string",
                "format": "date-time"
              },
              "message": {
                "type": "string"
              },
              "reason": {
                "type": "string"
              },
              "severity": {
                "type": "string"
              },
              "status": {
                "type": "string"
              },
              "type": {
                "type": "string"
              }
            },
            "additionalProperties": false
          }
        },
        "failureMessage": {
          "type": "string"
        },
        "failureReason": {
          "type": "string"
        },
        "instances": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "instanceID": {
                "type": "string"
              },
              "version": {
                "type": "string"
              }
            },
            "additionalProperties": false
          }
        },
        "launchTemplateID": {
          "type": "string"
        },
        "ready": {
          "type": "boolean"
        },
        "replicas": {
          "type": "integer",
          "format": "int32"
        }
      },
      "additionalProperties": false
    }
  }
}
