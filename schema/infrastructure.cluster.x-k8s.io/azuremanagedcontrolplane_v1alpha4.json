{
  "type": "object",
  "properties": {
    "apiVersion": {
      "type": "string"
    },
    "kind": {
      "type": "string"
    },
    "metadata": {
      "type": "object"
    },
    "spec": {
      "type": "object",
      "required": [
        "location",
        "resourceGroupName",
        "sshPublicKey",
        "version"
      ],
      "properties": {
        "aadProfile": {
          "type": "object",
          "required": [
            "adminGroupObjectIDs",
            "managed"
          ],
          "properties": {
            "adminGroupObjectIDs": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "managed": {
              "type": "boolean"
            }
          },
          "additionalProperties": false
        },
        "additionalTags": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          }
        },
        "apiServerAccessProfile": {
          "type": "object",
          "properties": {
            "authorizedIPRanges": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "enablePrivateCluster": {
              "type": "boolean"
            },
            "enablePrivateClusterPublicFQDN": {
              "type": "boolean"
            },
            "privateDNSZone": {
              "type": "string",
              "enum": [
                "System",
                "None"
              ]
            }
          },
          "additionalProperties": false
        },
        "controlPlaneEndpoint": {
          "type": "object",
          "required": [
            "host",
            "port"
          ],
          "properties": {
            "host": {
              "type": "string"
            },
            "port": {
              "type": "integer",
              "format": "int32"
            }
          },
          "additionalProperties": false
        },
        "dnsServiceIP": {
          "type": "string"
        },
        "identityRef": {
          "type": "object",
          "properties": {
            "apiVersion": {
              "type": "string"
            },
            "fieldPath": {
              "type": "string"
            },
            "kind": {
              "type": "string"
            },
            "name": {
              "type": "string"
            },
            "namespace": {
              "type": "string"
            },
            "resourceVersion": {
              "type": "string"
            },
            "uid": {
              "type": "string"
            }
          },
          "additionalProperties": false,
          "x-kubernetes-map-type": "atomic"
        },
        "loadBalancerProfile": {
          "type": "object",
          "properties": {
            "allocatedOutboundPorts": {
              "type": "integer",
              "format": "int32"
            },
            "idleTimeoutInMinutes": {
              "type": "integer",
              "format": "int32"
            },
            "managedOutboundIPs": {
              "type": "integer",
              "format": "int32"
            },
            "outboundIPPrefixes": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "outboundIPs": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          },
          "additionalProperties": false
        },
        "loadBalancerSKU": {
          "type": "string",
          "enum": [
            "Basic",
            "Standard"
          ]
        },
        "location": {
          "type": "string"
        },
        "networkPlugin": {
          "type": "string",
          "enum": [
            "azure",
            "kubenet"
          ]
        },
        "networkPolicy": {
          "type": "string",
          "enum": [
            "azure",
            "calico"
          ]
        },
        "nodeResourceGroupName": {
          "type": "string"
        },
        "resourceGroupName": {
          "type": "string"
        },
        "sku": {
          "type": "object",
          "required": [
            "tier"
          ],
          "properties": {
            "tier": {
              "type": "string",
              "enum": [
                "Free",
                "Paid"
              ]
            }
          },
          "additionalProperties": false
        },
        "sshPublicKey": {
          "type": "string"
        },
        "subscriptionID": {
          "type": "string"
        },
        "version": {
          "type": "string",
          "minLength": 2
        },
        "virtualNetwork": {
          "type": "object",
          "required": [
            "cidrBlock",
            "name"
          ],
          "properties": {
            "cidrBlock": {
              "type": "string"
            },
            "name": {
              "type": "string"
            },
            "subnet": {
              "type": "object",
              "required": [
                "cidrBlock",
                "name"
              ],
              "properties": {
                "cidrBlock": {
                  "type": "string"
                },
                "name": {
                  "type": "string"
                }
              },
              "additionalProperties": false
            }
          },
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    },
    "status": {
      "type": "object",
      "properties": {
        "initialized": {
          "type": "boolean"
        },
        "longRunningOperationStates": {
          "type": "array",
          "items": {
            "type": "object",
            "required": [
              "name",
              "serviceName",
              "type"
            ],
            "properties": {
              "data": {
                "type": "string"
              },
              "name": {
                "type": "string"
              },
              "resourceGroup": {
                "type": "string"
              },
              "serviceName": {
                "type": "string"
              },
              "type": {
                "type": "string"
              }
            },
            "additionalProperties": false
          }
        },
        "ready": {
          "type": "boolean"
        }
      },
      "additionalProperties": false
    }
  }
}
