{
  "type": "object",
  "properties": {
    "apiVersion": {
      "type": "string"
    },
    "kind": {
      "type": "string"
    },
    "metadata": {
      "type": "object"
    },
    "spec": {
      "type": "object",
      "required": [
        "identityRef",
        "reclaimPolicy"
      ],
      "properties": {
        "floatingIPNetwork": {
          "type": "object",
          "maxProperties": 1,
          "minProperties": 1,
          "properties": {
            "filter": {
              "type": "object",
              "minProperties": 1,
              "properties": {
                "description": {
                  "type": "string"
                },
                "name": {
                  "type": "string"
                },
                "notTags": {
                  "type": "array",
                  "items": {
                    "type": "string",
                    "minLength": 1,
                    "pattern": "^[^,]+$"
                  },
                  "x-kubernetes-list-type": "set"
                },
                "notTagsAny": {
                  "type": "array",
                  "items": {
                    "type": "string",
                    "minLength": 1,
                    "pattern": "^[^,]+$"
                  },
                  "x-kubernetes-list-type": "set"
                },
                "projectID": {
                  "type": "string"
                },
                "tags": {
                  "type": "array",
                  "items": {
                    "type": "string",
                    "minLength": 1,
                    "pattern": "^[^,]+$"
                  },
                  "x-kubernetes-list-type": "set"
                },
                "tagsAny": {
                  "type": "array",
                  "items": {
                    "type": "string",
                    "minLength": 1,
                    "pattern": "^[^,]+$"
                  },
                  "x-kubernetes-list-type": "set"
                }
              },
              "additionalProperties": false
            },
            "id": {
              "type": "string",
              "format": "uuid"
            }
          },
          "additionalProperties": false
        },
        "identityRef": {
          "type": "object",
          "required": [
            "cloudName",
            "name"
          ],
          "properties": {
            "cloudName": {
              "type": "string"
            },
            "name": {
              "type": "string"
            },
            "region": {
              "type": "string"
            }
          },
          "additionalProperties": false,
          "x-kubernetes-validations": [
            {
              "rule": "(!has(self.region) \u0026\u0026 !has(oldSelf.region)) || self.region == oldSelf.region",
              "message": "region is immutable"
            }
          ]
        },
        "maxIPs": {
          "type": "integer"
        },
        "preAllocatedFloatingIPs": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "reclaimPolicy": {
          "type": "string",
          "enum": [
            "Retain",
            "Delete"
          ]
        }
      },
      "additionalProperties": false
    },
    "status": {
      "type": "object",
      "properties": {
        "availableIPs": {
          "type": "array",
          "default": [],
          "items": {
            "type": "string"
          }
        },
        "claimedIPs": {
          "type": "array",
          "default": [],
          "items": {
            "type": "string"
          }
        },
        "conditions": {
          "type": "array",
          "items": {
            "type": "object",
            "required": [
              "lastTransitionTime",
              "status",
              "type"
            ],
            "properties": {
              "lastTransitionTime": {
                "type": "string",
                "format": "date-time"
              },
              "message": {
                "type": "string",
                "maxLength": 10240,
                "minLength": 1
              },
              "reason": {
                "type": "string",
                "maxLength": 256,
                "minLength": 1
              },
              "severity": {
                "type": "string",
                "maxLength": 32
              },
              "status": {
                "type": "string"
              },
              "type": {
                "type": "string",
                "maxLength": 256,
                "minLength": 1
              }
            },
            "additionalProperties": false
          }
        },
        "failedIPs": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "floatingIPNetwork": {
          "type": "object",
          "required": [
            "id",
            "name"
          ],
          "properties": {
            "id": {
              "type": "string"
            },
            "name": {
              "type": "string"
            },
            "tags": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          },
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    }
  }
}
