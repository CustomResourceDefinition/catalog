{
  "type": "object",
  "properties": {
    "apiVersion": {
      "type": "string"
    },
    "kind": {
      "type": "string"
    },
    "metadata": {
      "type": "object"
    },
    "spec": {
      "type": "object",
      "required": [
        "region"
      ],
      "properties": {
        "network": {
          "type": "object",
          "properties": {
            "apiServerLoadbalancers": {
              "type": "object",
              "properties": {
                "algorithm": {
                  "type": "string",
                  "enum": [
                    "round_robin",
                    "least_connections"
                  ]
                },
                "healthCheck": {
                  "type": "object",
                  "properties": {
                    "healthyThreshold": {
                      "type": "integer",
                      "maximum": 10,
                      "minimum": 2
                    },
                    "interval": {
                      "type": "integer",
                      "maximum": 300,
                      "minimum": 3
                    },
                    "timeout": {
                      "type": "integer",
                      "maximum": 300,
                      "minimum": 3
                    },
                    "unhealthyThreshold": {
                      "type": "integer",
                      "maximum": 10,
                      "minimum": 2
                    }
                  },
                  "additionalProperties": false
                },
                "port": {
                  "type": "integer",
                  "maximum": 65535,
                  "minimum": 1
                }
              },
              "additionalProperties": false
            }
          },
          "additionalProperties": false
        },
        "region": {
          "type": "string"
        }
      },
      "additionalProperties": false
    },
    "status": {
      "type": "object",
      "properties": {
        "apiEndpoints": {
          "type": "array",
          "items": {
            "type": "object",
            "required": [
              "host",
              "port"
            ],
            "properties": {
              "host": {
                "type": "string"
              },
              "port": {
                "type": "integer"
              }
            },
            "additionalProperties": false
          }
        },
        "network": {
          "type": "object",
          "properties": {
            "apiServerLoadbalancersRef": {
              "type": "object",
              "properties": {
                "resourceId": {
                  "type": "string"
                },
                "resourceStatus": {
                  "type": "string"
                }
              },
              "additionalProperties": false
            }
          },
          "additionalProperties": false
        },
        "ready": {
          "type": "boolean"
        }
      },
      "additionalProperties": false
    }
  }
}
