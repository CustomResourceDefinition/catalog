{
  "type": "object",
  "properties": {
    "apiVersion": {
      "type": "string"
    },
    "kind": {
      "type": "string"
    },
    "metadata": {
      "type": "object"
    },
    "spec": {
      "type": "object",
      "properties": {
        "additionalTags": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          }
        },
        "amiType": {
          "type": "string",
          "default": "AL2_x86_64",
          "enum": [
            "AL2_x86_64",
            "AL2_x86_64_GPU",
            "AL2_ARM_64",
            "CUSTOM",
            "BOTTLEROCKET_ARM_64",
            "BOTTLEROCKET_x86_64",
            "BOTTLEROCKET_ARM_64_FIPS",
            "BOTTLEROCKET_x86_64_FIPS",
            "BOTTLEROCKET_ARM_64_NVIDIA",
            "BOTTLEROCKET_x86_64_NVIDIA",
            "WINDOWS_CORE_2019_x86_64",
            "WINDOWS_FULL_2019_x86_64",
            "WINDOWS_CORE_2022_x86_64",
            "WINDOWS_FULL_2022_x86_64",
            "AL2023_x86_64_STANDARD",
            "AL2023_ARM_64_STANDARD",
            "AL2023_x86_64_NEURON",
            "AL2023_x86_64_NVIDIA",
            "AL2023_ARM_64_NVIDIA"
          ]
        },
        "amiVersion": {
          "type": "string",
          "minLength": 2
        },
        "availabilityZoneSubnetType": {
          "type": "string",
          "enum": [
            "public",
            "private",
            "all"
          ]
        },
        "availabilityZones": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "awsLaunchTemplate": {
          "type": "object",
          "properties": {
            "additionalSecurityGroups": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "filters": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "required": [
                        "name",
                        "values"
                      ],
                      "properties": {
                        "name": {
                          "type": "string"
                        },
                        "values": {
                          "type": "array",
                          "items": {
                            "type": "string"
                          }
                        }
                      },
                      "additionalProperties": false
                    }
                  },
                  "id": {
                    "type": "string"
                  }
                },
                "additionalProperties": false
              }
            },
            "ami": {
              "type": "object",
              "properties": {
                "eksLookupType": {
                  "type": "string",
                  "enum": [
                    "AmazonLinux",
                    "AmazonLinuxGPU",
                    "AmazonLinux2023",
                    "AmazonLinux2023GPU"
                  ]
                },
                "id": {
                  "type": "string"
                }
              },
              "additionalProperties": false
            },
            "capacityReservationId": {
              "type": "string"
            },
            "capacityReservationPreference": {
              "type": "string",
              "allOf": [
                {
                  "enum": [
                    "",
                    "None",
                    "CapacityReservationsOnly",
                    "Open"
                  ]
                },
                {
                  "enum": [
                    "",
                    "None",
                    "CapacityReservationsOnly",
                    "Open"
                  ]
                }
              ]
            },
            "iamInstanceProfile": {
              "type": "string"
            },
            "imageLookupBaseOS": {
              "type": "string"
            },
            "imageLookupFormat": {
              "type": "string"
            },
            "imageLookupOrg": {
              "type": "string"
            },
            "instanceMetadataOptions": {
              "type": "object",
              "properties": {
                "httpEndpoint": {
                  "type": "string",
                  "default": "enabled",
                  "enum": [
                    "enabled",
                    "disabled"
                  ]
                },
                "httpPutResponseHopLimit": {
                  "type": "integer",
                  "format": "int64",
                  "default": 1,
                  "maximum": 64,
                  "minimum": 1
                },
                "httpTokens": {
                  "type": "string",
                  "default": "optional",
                  "enum": [
                    "optional",
                    "required"
                  ]
                },
                "instanceMetadataTags": {
                  "type": "string",
                  "default": "disabled",
                  "enum": [
                    "enabled",
                    "disabled"
                  ]
                }
              },
              "additionalProperties": false
            },
            "instanceType": {
              "type": "string"
            },
            "marketType": {
              "type": "string",
              "enum": [
                "OnDemand",
                "Spot",
                "CapacityBlock"
              ]
            },
            "name": {
              "type": "string"
            },
            "nonRootVolumes": {
              "type": "array",
              "items": {
                "type": "object",
                "required": [
                  "size"
                ],
                "properties": {
                  "deviceName": {
                    "type": "string"
                  },
                  "encrypted": {
                    "type": "boolean"
                  },
                  "encryptionKey": {
                    "type": "string"
                  },
                  "iops": {
                    "type": "integer",
                    "format": "int64"
                  },
                  "size": {
                    "type": "integer",
                    "format": "int64",
                    "minimum": 8
                  },
                  "throughput": {
                    "type": "integer",
                    "format": "int64"
                  },
                  "type": {
                    "type": "string"
                  }
                },
                "additionalProperties": false
              }
            },
            "privateDnsName": {
              "type": "object",
              "properties": {
                "enableResourceNameDnsAAAARecord": {
                  "type": "boolean"
                },
                "enableResourceNameDnsARecord": {
                  "type": "boolean"
                },
                "hostnameType": {
                  "type": "string",
                  "enum": [
                    "ip-name",
                    "resource-name"
                  ]
                }
              },
              "additionalProperties": false
            },
            "rootVolume": {
              "type": "object",
              "required": [
                "size"
              ],
              "properties": {
                "deviceName": {
                  "type": "string"
                },
                "encrypted": {
                  "type": "boolean"
                },
                "encryptionKey": {
                  "type": "string"
                },
                "iops": {
                  "type": "integer",
                  "format": "int64"
                },
                "size": {
                  "type": "integer",
                  "format": "int64",
                  "minimum": 8
                },
                "throughput": {
                  "type": "integer",
                  "format": "int64"
                },
                "type": {
                  "type": "string"
                }
              },
              "additionalProperties": false
            },
            "spotMarketOptions": {
              "type": "object",
              "properties": {
                "maxPrice": {
                  "type": "string"
                }
              },
              "additionalProperties": false
            },
            "sshKeyName": {
              "type": "string"
            },
            "versionNumber": {
              "type": "integer",
              "format": "int64"
            }
          },
          "additionalProperties": false
        },
        "capacityType": {
          "type": "string",
          "default": "onDemand",
          "enum": [
            "onDemand",
            "spot"
          ]
        },
        "diskSize": {
          "type": "integer",
          "format": "int32"
        },
        "eksNodegroupName": {
          "type": "string"
        },
        "instanceType": {
          "type": "string"
        },
        "labels": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          }
        },
        "lifecycleHooks": {
          "type": "array",
          "items": {
            "type": "object",
            "required": [
              "lifecycleTransition",
              "name"
            ],
            "properties": {
              "defaultResult": {
                "type": "string",
                "enum": [
                  "CONTINUE",
                  "ABANDON"
                ]
              },
              "heartbeatTimeout": {
                "type": "string",
                "format": "duration"
              },
              "lifecycleTransition": {
                "type": "string",
                "enum": [
                  "autoscaling:EC2_INSTANCE_LAUNCHING",
                  "autoscaling:EC2_INSTANCE_TERMINATING"
                ]
              },
              "name": {
                "type": "string"
              },
              "notificationMetadata": {
                "type": "string"
              },
              "notificationTargetARN": {
                "type": "string"
              },
              "roleARN": {
                "type": "string"
              }
            },
            "additionalProperties": false
          }
        },
        "providerIDList": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "remoteAccess": {
          "type": "object",
          "properties": {
            "public": {
              "type": "boolean"
            },
            "sourceSecurityGroups": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "sshKeyName": {
              "type": "string"
            }
          },
          "additionalProperties": false
        },
        "roleAdditionalPolicies": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "roleName": {
          "type": "string"
        },
        "rolePath": {
          "type": "string"
        },
        "rolePermissionsBoundary": {
          "type": "string"
        },
        "scaling": {
          "type": "object",
          "properties": {
            "maxSize": {
              "type": "integer",
              "format": "int32"
            },
            "minSize": {
              "type": "integer",
              "format": "int32"
            }
          },
          "additionalProperties": false
        },
        "subnetIDs": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "taints": {
          "type": "array",
          "items": {
            "type": "object",
            "required": [
              "effect",
              "key",
              "value"
            ],
            "properties": {
              "effect": {
                "type": "string",
                "enum": [
                  "no-schedule",
                  "no-execute",
                  "prefer-no-schedule"
                ]
              },
              "key": {
                "type": "string"
              },
              "value": {
                "type": "string"
              }
            },
            "additionalProperties": false
          }
        },
        "updateConfig": {
          "type": "object",
          "properties": {
            "maxUnavailable": {
              "type": "integer",
              "maximum": 100,
              "minimum": 1
            },
            "maxUnavailablePercentage": {
              "type": "integer",
              "maximum": 100,
              "minimum": 1
            }
          },
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    },
    "status": {
      "type": "object",
      "required": [
        "ready"
      ],
      "properties": {
        "conditions": {
          "type": "array",
          "items": {
            "type": "object",
            "required": [
              "lastTransitionTime",
              "status",
              "type"
            ],
            "properties": {
              "lastTransitionTime": {
                "type": "string",
                "format": "date-time"
              },
              "message": {
                "type": "string",
                "maxLength": 10240,
                "minLength": 1
              },
              "reason": {
                "type": "string",
                "maxLength": 256,
                "minLength": 1
              },
              "severity": {
                "type": "string",
                "maxLength": 32
              },
              "status": {
                "type": "string"
              },
              "type": {
                "type": "string",
                "maxLength": 256,
                "minLength": 1
              }
            },
            "additionalProperties": false
          }
        },
        "failureMessage": {
          "type": "string"
        },
        "failureReason": {
          "type": "string"
        },
        "launchTemplateID": {
          "type": "string"
        },
        "launchTemplateVersion": {
          "type": "string"
        },
        "ready": {
          "type": "boolean",
          "default": false
        },
        "replicas": {
          "type": "integer",
          "format": "int32"
        }
      },
      "additionalProperties": false
    }
  }
}
