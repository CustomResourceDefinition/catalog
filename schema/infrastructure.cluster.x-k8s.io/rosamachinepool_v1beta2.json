{
  "type": "object",
  "properties": {
    "apiVersion": {
      "type": "string"
    },
    "kind": {
      "type": "string"
    },
    "metadata": {
      "type": "object"
    },
    "spec": {
      "type": "object",
      "required": [
        "instanceType",
        "nodePoolName"
      ],
      "properties": {
        "additionalSecurityGroups": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "additionalTags": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          }
        },
        "autoRepair": {
          "type": "boolean",
          "default": true
        },
        "autoscaling": {
          "type": "object",
          "properties": {
            "maxReplicas": {
              "type": "integer",
              "minimum": 1
            },
            "minReplicas": {
              "type": "integer",
              "minimum": 1
            }
          },
          "additionalProperties": false
        },
        "availabilityZone": {
          "type": "string"
        },
        "instanceType": {
          "type": "string"
        },
        "labels": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          }
        },
        "nodeDrainGracePeriod": {
          "type": "string"
        },
        "nodePoolName": {
          "type": "string",
          "maxLength": 15,
          "pattern": "^[a-z]([-a-z0-9]*[a-z0-9])?$",
          "x-kubernetes-validations": [
            {
              "rule": "self == oldSelf",
              "message": "nodepoolName is immutable"
            }
          ]
        },
        "providerIDList": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "subnet": {
          "type": "string",
          "x-kubernetes-validations": [
            {
              "rule": "self == oldSelf",
              "message": "subnet is immutable"
            }
          ]
        },
        "taints": {
          "type": "array",
          "items": {
            "type": "object",
            "required": [
              "effect",
              "key"
            ],
            "properties": {
              "effect": {
                "type": "string",
                "enum": [
                  "NoSchedule",
                  "PreferNoSchedule",
                  "NoExecute"
                ]
              },
              "key": {
                "type": "string"
              },
              "value": {
                "type": "string",
                "pattern": "^(([A-Za-z0-9][-A-Za-z0-9_.]*)?[A-Za-z0-9])?$"
              }
            },
            "additionalProperties": false
          }
        },
        "tuningConfigs": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "updateConfig": {
          "type": "object",
          "properties": {
            "rollingUpdate": {
              "type": "object",
              "properties": {
                "maxSurge": {
                  "default": 1,
                  "pattern": "^((100|[0-9]{1,2})%|[0-9]+)$",
                  "anyOf": [
                    {
                      "type": "integer"
                    },
                    {
                      "type": "string"
                    }
                  ],
                  "x-kubernetes-int-or-string": true
                },
                "maxUnavailable": {
                  "default": 0,
                  "pattern": "^((100|[0-9]{1,2})%|[0-9]+)$",
                  "anyOf": [
                    {
                      "type": "integer"
                    },
                    {
                      "type": "string"
                    }
                  ],
                  "x-kubernetes-int-or-string": true
                }
              },
              "additionalProperties": false
            }
          },
          "additionalProperties": false
        },
        "version": {
          "type": "string"
        },
        "volumeSize": {
          "type": "integer",
          "maximum": 16384,
          "minimum": 75
        }
      },
      "additionalProperties": false
    },
    "status": {
      "type": "object",
      "required": [
        "ready"
      ],
      "properties": {
        "availableUpgrades": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "conditions": {
          "type": "array",
          "items": {
            "type": "object",
            "required": [
              "lastTransitionTime",
              "status",
              "type"
            ],
            "properties": {
              "lastTransitionTime": {
                "type": "string",
                "format": "date-time"
              },
              "message": {
                "type": "string",
                "maxLength": 10240,
                "minLength": 1
              },
              "reason": {
                "type": "string",
                "maxLength": 256,
                "minLength": 1
              },
              "severity": {
                "type": "string",
                "maxLength": 32
              },
              "status": {
                "type": "string"
              },
              "type": {
                "type": "string",
                "maxLength": 256,
                "minLength": 1
              }
            },
            "additionalProperties": false
          }
        },
        "failureMessage": {
          "type": "string"
        },
        "id": {
          "type": "string"
        },
        "ready": {
          "type": "boolean",
          "default": false
        },
        "replicas": {
          "type": "integer",
          "format": "int32"
        }
      },
      "additionalProperties": false
    }
  }
}
