{
  "properties": {
    "apiVersion": {
      "type": "string"
    },
    "kind": {
      "type": "string"
    },
    "metadata": {
      "type": "object"
    },
    "spec": {
      "additionalProperties": false,
      "properties": {
        "clusterName": {
          "type": "string"
        },
        "clusterNetwork": {
          "additionalProperties": false,
          "properties": {
            "pod": {
              "additionalProperties": false,
              "properties": {
                "cidrBlock": {
                  "type": "string"
                }
              },
              "type": "object"
            },
            "privateCluster": {
              "additionalProperties": false,
              "properties": {
                "controlPlaneCidrBlock": {
                  "type": "string"
                },
                "controlPlaneGlobalAccess": {
                  "type": "boolean"
                },
                "disableDefaultSNAT": {
                  "type": "boolean"
                },
                "enablePrivateEndpoint": {
                  "type": "boolean"
                },
                "enablePrivateNodes": {
                  "type": "boolean"
                }
              },
              "type": "object"
            },
            "service": {
              "additionalProperties": false,
              "properties": {
                "cidrBlock": {
                  "type": "string"
                }
              },
              "type": "object"
            },
            "useIPAliases": {
              "type": "boolean"
            }
          },
          "type": "object"
        },
        "controlPlaneVersion": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "enableAutopilot": {
          "type": "boolean"
        },
        "endpoint": {
          "additionalProperties": false,
          "properties": {
            "host": {
              "type": "string"
            },
            "port": {
              "format": "int32",
              "type": "integer"
            }
          },
          "required": [
            "host",
            "port"
          ],
          "type": "object"
        },
        "location": {
          "type": "string"
        },
        "master_authorized_networks_config": {
          "additionalProperties": false,
          "properties": {
            "cidr_blocks": {
              "items": {
                "additionalProperties": false,
                "properties": {
                  "cidr_block": {
                    "pattern": "^(?:[0-9]{1,3}\\.){3}[0-9]{1,3}(?:\\/([0-9]|[1-2][0-9]|3[0-2]))?$|^([a-fA-F0-9:]+:+)+[a-fA-F0-9]+\\/[0-9]{1,3}$",
                    "type": "string"
                  },
                  "display_name": {
                    "type": "string"
                  }
                },
                "type": "object"
              },
              "type": "array"
            },
            "gcp_public_cidrs_access_enabled": {
              "type": "boolean"
            }
          },
          "type": "object"
        },
        "project": {
          "type": "string"
        },
        "releaseChannel": {
          "enum": [
            "rapid",
            "regular",
            "stable"
          ],
          "type": "string"
        }
      },
      "required": [
        "location",
        "project"
      ],
      "type": "object"
    },
    "status": {
      "additionalProperties": false,
      "properties": {
        "conditions": {
          "items": {
            "additionalProperties": false,
            "properties": {
              "lastTransitionTime": {
                "format": "date-time",
                "type": "string"
              },
              "message": {
                "type": "string"
              },
              "reason": {
                "type": "string"
              },
              "severity": {
                "type": "string"
              },
              "status": {
                "type": "string"
              },
              "type": {
                "type": "string"
              }
            },
            "required": [
              "lastTransitionTime",
              "status",
              "type"
            ],
            "type": "object"
          },
          "type": "array"
        },
        "currentVersion": {
          "type": "string"
        },
        "initialized": {
          "type": "boolean"
        },
        "ready": {
          "default": false,
          "type": "boolean"
        }
      },
      "required": [
        "ready"
      ],
      "type": "object"
    }
  },
  "type": "object"
}
