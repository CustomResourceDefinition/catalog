{
  "type": "object",
  "properties": {
    "apiVersion": {
      "type": "string"
    },
    "kind": {
      "type": "string"
    },
    "metadata": {
      "type": "object"
    },
    "spec": {
      "type": "object",
      "required": [
        "isDefault"
      ],
      "properties": {
        "apiKey": {
          "type": "object"
        },
        "isDefault": {
          "type": "boolean"
        },
        "jwt": {
          "type": "object",
          "required": [
            "appIdClaim"
          ],
          "properties": {
            "appIdClaim": {
              "type": "string"
            },
            "forwardHeaders": {
              "type": "object",
              "additionalProperties": {
                "type": "string"
              }
            },
            "jwksFile": {
              "type": "string"
            },
            "jwksUrl": {
              "type": "string",
              "x-kubernetes-validations": [
                {
                  "rule": "isURL(self)",
                  "message": "must be a valid URL"
                }
              ]
            },
            "publicKey": {
              "type": "string"
            },
            "signingSecretName": {
              "type": "string",
              "maxLength": 253
            },
            "stripAuthorizationHeader": {
              "type": "boolean"
            },
            "tokenNameClaim": {
              "type": "string"
            },
            "tokenQueryKey": {
              "type": "string"
            }
          },
          "additionalProperties": false,
          "x-kubernetes-validations": [
            {
              "rule": "[has(self.signingSecretName), has(self.publicKey), has(self.jwksFile), has(self.jwksUrl)].filter(x, x).size() == 1",
              "message": "exactly one of signingSecretName, publicKey, jwksFile, or jwksUrl must be specified"
            }
          ]
        }
      },
      "additionalProperties": false,
      "x-kubernetes-validations": [
        {
          "rule": "(has(self.apiKey) \u0026\u0026 !has(self.jwt)) || (!has(self.apiKey) \u0026\u0026 has(self.jwt))",
          "message": "exactly one of apiKey or jwt must be specified"
        }
      ]
    },
    "status": {
      "type": "object",
      "properties": {
        "hash": {
          "type": "string"
        },
        "syncedAt": {
          "type": "string",
          "format": "date-time"
        },
        "version": {
          "type": "string"
        }
      },
      "additionalProperties": false
    }
  }
}
