{
  "type": "object",
  "properties": {
    "apiVersion": {
      "type": "string"
    },
    "kind": {
      "type": "string"
    },
    "metadata": {
      "type": "object"
    },
    "spec": {
      "type": "object",
      "required": [
        "apiServerRef",
        "clusterRole"
      ],
      "properties": {
        "apiServerRef": {
          "type": "object",
          "properties": {
            "name": {
              "type": "string",
              "default": ""
            }
          },
          "additionalProperties": false,
          "x-kubernetes-map-type": "atomic"
        },
        "cel": {
          "type": "string"
        },
        "clusterRole": {
          "type": "string"
        },
        "identityPools": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "serviceAccounts": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "srnCluster": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "srnInstance": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "srnNamespace": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "srnOrganization": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "srnSecret": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "srnServiceAccount": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "srnSubscription": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "srnTenant": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "srnTopicDomain": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "srnTopicName": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "users": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      },
      "additionalProperties": false
    },
    "status": {
      "type": "object",
      "properties": {
        "conditions": {
          "type": "array",
          "items": {
            "type": "object",
            "required": [
              "lastTransitionTime",
              "message",
              "reason",
              "status",
              "type"
            ],
            "properties": {
              "lastTransitionTime": {
                "type": "string",
                "format": "date-time"
              },
              "message": {
                "type": "string",
                "maxLength": 32768
              },
              "observedGeneration": {
                "type": "integer",
                "format": "int64",
                "minimum": 0
              },
              "reason": {
                "type": "string",
                "maxLength": 1024,
                "minLength": 1,
                "pattern": "^[A-Za-z]([A-Za-z0-9_,:]*[A-Za-z0-9_])?$"
              },
              "status": {
                "type": "string",
                "enum": [
                  "True",
                  "False",
                  "Unknown"
                ]
              },
              "type": {
                "type": "string",
                "maxLength": 316,
                "pattern": "^([a-z0-9]([-a-z0-9]*[a-z0-9])?(\\.[a-z0-9]([-a-z0-9]*[a-z0-9])?)*/)?(([A-Za-z0-9][-A-Za-z0-9_.]*)?[A-Za-z0-9])$"
              }
            },
            "additionalProperties": false
          }
        },
        "failedClusters": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "observedGeneration": {
          "type": "integer",
          "format": "int64"
        },
        "syncedClusters": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          }
        }
      },
      "additionalProperties": false
    }
  }
}
