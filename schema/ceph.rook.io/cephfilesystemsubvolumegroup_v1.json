{
  "type": "object",
  "required": [
    "metadata",
    "spec"
  ],
  "properties": {
    "apiVersion": {
      "type": "string"
    },
    "kind": {
      "type": "string"
    },
    "metadata": {
      "type": "object"
    },
    "spec": {
      "type": "object",
      "required": [
        "filesystemName"
      ],
      "properties": {
        "clusterID": {
          "type": "string",
          "maxLength": 36,
          "minLength": 1,
          "pattern": "^[a-zA-Z0-9_-]+$",
          "x-kubernetes-validations": [
            {
              "rule": "self == oldSelf",
              "message": "ClusterID is immutable"
            }
          ]
        },
        "dataPoolName": {
          "type": "string"
        },
        "filesystemName": {
          "type": "string",
          "x-kubernetes-validations": [
            {
              "rule": "self == oldSelf",
              "message": "filesystemName is immutable"
            }
          ]
        },
        "name": {
          "type": "string",
          "x-kubernetes-validations": [
            {
              "rule": "self == oldSelf",
              "message": "name is immutable"
            }
          ]
        },
        "pinning": {
          "type": "object",
          "properties": {
            "distributed": {
              "type": "integer",
              "maximum": 1,
              "minimum": 0,
              "nullable": true
            },
            "export": {
              "type": "integer",
              "maximum": 256,
              "minimum": -1,
              "nullable": true
            },
            "random": {
              "type": "number",
              "maximum": 1,
              "minimum": 0,
              "nullable": true
            }
          },
          "additionalProperties": false,
          "x-kubernetes-validations": [
            {
              "rule": "(has(self.export) \u0026\u0026 !has(self.distributed) \u0026\u0026 !has(self.random)) || (!has(self.export) \u0026\u0026 has(self.distributed) \u0026\u0026 !has(self.random)) || (!has(self.export) \u0026\u0026 !has(self.distributed) \u0026\u0026 has(self.random)) || (!has(self.export) \u0026\u0026 !has(self.distributed) \u0026\u0026 !has(self.random))",
              "message": "only one pinning type should be set"
            }
          ]
        },
        "quota": {
          "pattern": "^(\\+|-)?(([0-9]+(\\.[0-9]*)?)|(\\.[0-9]+))(([KMGTPE]i)|[numkMGTPE]|([eE](\\+|-)?(([0-9]+(\\.[0-9]*)?)|(\\.[0-9]+))))?$",
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "string"
            }
          ],
          "x-kubernetes-int-or-string": true
        }
      },
      "additionalProperties": false
    },
    "status": {
      "type": "object",
      "properties": {
        "info": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          },
          "nullable": true
        },
        "observedGeneration": {
          "type": "integer",
          "format": "int64"
        },
        "phase": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "x-kubernetes-preserve-unknown-fields": true
    }
  }
}
