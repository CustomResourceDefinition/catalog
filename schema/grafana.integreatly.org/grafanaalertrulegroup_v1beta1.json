{
  "type": "object",
  "required": [
    "spec"
  ],
  "properties": {
    "apiVersion": {
      "type": "string"
    },
    "kind": {
      "type": "string"
    },
    "metadata": {
      "type": "object"
    },
    "spec": {
      "type": "object",
      "required": [
        "instanceSelector",
        "interval",
        "rules"
      ],
      "properties": {
        "allowCrossNamespaceImport": {
          "type": "boolean",
          "default": false
        },
        "editable": {
          "type": "boolean",
          "x-kubernetes-validations": [
            {
              "rule": "self == oldSelf",
              "message": "Value is immutable"
            }
          ]
        },
        "folderRef": {
          "type": "string"
        },
        "folderUID": {
          "type": "string"
        },
        "instanceSelector": {
          "type": "object",
          "properties": {
            "matchExpressions": {
              "type": "array",
              "items": {
                "type": "object",
                "required": [
                  "key",
                  "operator"
                ],
                "properties": {
                  "key": {
                    "type": "string"
                  },
                  "operator": {
                    "type": "string"
                  },
                  "values": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    },
                    "x-kubernetes-list-type": "atomic"
                  }
                },
                "additionalProperties": false
              },
              "x-kubernetes-list-type": "atomic"
            },
            "matchLabels": {
              "type": "object",
              "additionalProperties": {
                "type": "string"
              }
            }
          },
          "additionalProperties": false,
          "x-kubernetes-map-type": "atomic",
          "x-kubernetes-validations": [
            {
              "rule": "self == oldSelf",
              "message": "spec.instanceSelector is immutable"
            }
          ]
        },
        "interval": {
          "type": "string",
          "format": "duration",
          "pattern": "^([0-9]+(\\.[0-9]+)?(ns|us|µs|ms|s|m|h))+$"
        },
        "name": {
          "type": "string"
        },
        "resyncPeriod": {
          "type": "string",
          "default": "10m0s",
          "pattern": "^([0-9]+(\\.[0-9]+)?(ns|us|µs|ms|s|m|h))+$"
        },
        "rules": {
          "type": "array",
          "items": {
            "type": "object",
            "required": [
              "condition",
              "data",
              "execErrState",
              "for",
              "noDataState",
              "title",
              "uid"
            ],
            "properties": {
              "annotations": {
                "type": "object",
                "additionalProperties": {
                  "type": "string"
                }
              },
              "condition": {
                "type": "string"
              },
              "data": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "datasourceUid": {
                      "type": "string"
                    },
                    "model": {
                      "x-kubernetes-preserve-unknown-fields": true
                    },
                    "queryType": {
                      "type": "string"
                    },
                    "refId": {
                      "type": "string"
                    },
                    "relativeTimeRange": {
                      "type": "object",
                      "properties": {
                        "from": {
                          "type": "integer",
                          "format": "int64"
                        },
                        "to": {
                          "type": "integer",
                          "format": "int64"
                        }
                      },
                      "additionalProperties": false
                    }
                  },
                  "additionalProperties": false
                }
              },
              "execErrState": {
                "type": "string",
                "enum": [
                  "OK",
                  "Alerting",
                  "Error",
                  "KeepLast"
                ]
              },
              "for": {
                "type": "string",
                "format": "duration",
                "pattern": "^([0-9]+(\\.[0-9]+)?(ns|us|µs|ms|s|m|h))+$"
              },
              "isPaused": {
                "type": "boolean"
              },
              "labels": {
                "type": "object",
                "additionalProperties": {
                  "type": "string"
                }
              },
              "noDataState": {
                "type": "string",
                "enum": [
                  "Alerting",
                  "NoData",
                  "OK",
                  "KeepLast"
                ]
              },
              "notificationSettings": {
                "type": "object",
                "required": [
                  "receiver"
                ],
                "properties": {
                  "group_by": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  },
                  "group_interval": {
                    "type": "string"
                  },
                  "group_wait": {
                    "type": "string"
                  },
                  "mute_time_intervals": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  },
                  "receiver": {
                    "type": "string"
                  },
                  "repeat_interval": {
                    "type": "string"
                  }
                },
                "additionalProperties": false
              },
              "record": {
                "type": "object",
                "required": [
                  "from",
                  "metric"
                ],
                "properties": {
                  "from": {
                    "type": "string"
                  },
                  "metric": {
                    "type": "string"
                  }
                },
                "additionalProperties": false
              },
              "title": {
                "type": "string",
                "maxLength": 190,
                "minLength": 1,
                "example": "Always firing"
              },
              "uid": {
                "type": "string",
                "maxLength": 40,
                "pattern": "^[a-zA-Z0-9-_]+$"
              }
            },
            "additionalProperties": false
          }
        }
      },
      "additionalProperties": false,
      "x-kubernetes-validations": [
        {
          "rule": "(has(self.folderUID) \u0026\u0026 !(has(self.folderRef))) || (has(self.folderRef) \u0026\u0026 !(has(self.folderUID)))",
          "message": "Only one of FolderUID or FolderRef can be set"
        },
        {
          "rule": "((!has(oldSelf.editable) \u0026\u0026 !has(self.editable)) || (has(oldSelf.editable) \u0026\u0026 has(self.editable)))",
          "message": "spec.editable is immutable"
        },
        {
          "rule": "!oldSelf.allowCrossNamespaceImport || (oldSelf.allowCrossNamespaceImport \u0026\u0026 self.allowCrossNamespaceImport)",
          "message": "disabling spec.allowCrossNamespaceImport requires a recreate to ensure desired state"
        }
      ]
    },
    "status": {
      "type": "object",
      "properties": {
        "conditions": {
          "type": "array",
          "items": {
            "type": "object",
            "required": [
              "lastTransitionTime",
              "message",
              "reason",
              "status",
              "type"
            ],
            "properties": {
              "lastTransitionTime": {
                "type": "string",
                "format": "date-time"
              },
              "message": {
                "type": "string",
                "maxLength": 32768
              },
              "observedGeneration": {
                "type": "integer",
                "format": "int64",
                "minimum": 0
              },
              "reason": {
                "type": "string",
                "maxLength": 1024,
                "minLength": 1,
                "pattern": "^[A-Za-z]([A-Za-z0-9_,:]*[A-Za-z0-9_])?$"
              },
              "status": {
                "type": "string",
                "enum": [
                  "True",
                  "False",
                  "Unknown"
                ]
              },
              "type": {
                "type": "string",
                "maxLength": 316,
                "pattern": "^([a-z0-9]([-a-z0-9]*[a-z0-9])?(\\.[a-z0-9]([-a-z0-9]*[a-z0-9])?)*/)?(([A-Za-z0-9][-A-Za-z0-9_.]*)?[A-Za-z0-9])$"
              }
            },
            "additionalProperties": false
          }
        },
        "lastResync": {
          "type": "string",
          "format": "date-time"
        }
      },
      "additionalProperties": false
    }
  }
}
