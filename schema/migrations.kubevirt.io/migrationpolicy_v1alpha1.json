{
  "type": "object",
  "required": [
    "spec"
  ],
  "properties": {
    "apiVersion": {
      "type": "string"
    },
    "kind": {
      "type": "string"
    },
    "metadata": {
      "type": "object"
    },
    "spec": {
      "type": "object",
      "required": [
        "selectors"
      ],
      "properties": {
        "allowAutoConverge": {
          "type": "boolean"
        },
        "allowPostCopy": {
          "type": "boolean"
        },
        "allowWorkloadDisruption": {
          "type": "boolean"
        },
        "bandwidthPerMigration": {
          "pattern": "^(\\+|-)?(([0-9]+(\\.[0-9]*)?)|(\\.[0-9]+))(([KMGTPE]i)|[numkMGTPE]|([eE](\\+|-)?(([0-9]+(\\.[0-9]*)?)|(\\.[0-9]+))))?$",
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "string"
            }
          ],
          "x-kubernetes-int-or-string": true
        },
        "completionTimeoutPerGiB": {
          "type": "integer",
          "format": "int64"
        },
        "selectors": {
          "type": "object",
          "properties": {
            "namespaceSelector": {
              "type": "object",
              "additionalProperties": {
                "type": "string"
              }
            },
            "virtualMachineInstanceSelector": {
              "type": "object",
              "additionalProperties": {
                "type": "string"
              }
            }
          },
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    },
    "status": {
      "type": "object",
      "nullable": true
    }
  }
}
