{
  "type": "object",
  "properties": {
    "apiVersion": {
      "type": "string"
    },
    "kind": {
      "type": "string"
    },
    "metadata": {
      "type": "object"
    },
    "spec": {
      "type": "object",
      "properties": {
        "clusterName": {
          "type": "string",
          "maxLength": 63,
          "pattern": "^[a-z0-9]([-a-z0-9]*[a-z0-9])?$"
        },
        "deployOption": {
          "type": "object",
          "properties": {
            "mode": {
              "type": "string"
            }
          },
          "additionalProperties": false
        },
        "externalServerURLs": {
          "type": "array",
          "items": {
            "type": "object",
            "required": [
              "url"
            ],
            "properties": {
              "caBundle": {
                "type": "string",
                "format": "byte"
              },
              "url": {
                "type": "string"
              }
            },
            "additionalProperties": false
          }
        },
        "hubApiServerHostAlias": {
          "type": "object",
          "required": [
            "hostname",
            "ip"
          ],
          "properties": {
            "hostname": {
              "type": "string",
              "pattern": "^(([a-zA-Z0-9]|[a-zA-Z0-9][a-zA-Z0-9\\-]*[a-zA-Z0-9])\\.)*([A-Za-z0-9]|[A-Za-z0-9][A-Za-z0-9\\-]*[A-Za-z0-9])$"
            },
            "ip": {
              "type": "string",
              "pattern": "^(?:(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\\.){3}(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)$"
            }
          },
          "additionalProperties": false
        },
        "imagePullSpec": {
          "type": "string"
        },
        "namespace": {
          "type": "string",
          "maxLength": 57,
          "pattern": "^open-cluster-management-[-a-z0-9]*[a-z0-9]$"
        },
        "nodePlacement": {
          "type": "object",
          "properties": {
            "nodeSelector": {
              "type": "object",
              "additionalProperties": {
                "type": "string"
              }
            },
            "tolerations": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "effect": {
                    "type": "string"
                  },
                  "key": {
                    "type": "string"
                  },
                  "operator": {
                    "type": "string"
                  },
                  "tolerationSeconds": {
                    "type": "integer",
                    "format": "int64"
                  },
                  "value": {
                    "type": "string"
                  }
                },
                "additionalProperties": false
              }
            }
          },
          "additionalProperties": false
        },
        "priorityClassName": {
          "type": "string"
        },
        "registrationConfiguration": {
          "type": "object",
          "properties": {
            "bootstrapKubeConfigs": {
              "type": "object",
              "required": [
                "type"
              ],
              "properties": {
                "localSecretsConfig": {
                  "type": "object",
                  "required": [
                    "kubeConfigSecrets"
                  ],
                  "properties": {
                    "hubConnectionTimeoutSeconds": {
                      "type": "integer",
                      "format": "int32",
                      "default": 600,
                      "minimum": 180
                    },
                    "kubeConfigSecrets": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "required": [
                          "name"
                        ],
                        "properties": {
                          "name": {
                            "type": "string"
                          }
                        },
                        "additionalProperties": false
                      }
                    }
                  },
                  "additionalProperties": false
                },
                "type": {
                  "type": "string",
                  "default": "None",
                  "enum": [
                    "None",
                    "LocalSecrets"
                  ]
                }
              },
              "additionalProperties": false
            },
            "clientCertExpirationSeconds": {
              "type": "integer",
              "format": "int32"
            },
            "clusterAnnotations": {
              "type": "object",
              "additionalProperties": {
                "type": "string"
              }
            },
            "clusterClaimConfiguration": {
              "type": "object",
              "required": [
                "maxCustomClusterClaims"
              ],
              "properties": {
                "maxCustomClusterClaims": {
                  "type": "integer",
                  "format": "int32",
                  "default": 20
                },
                "reservedClusterClaimSuffixes": {
                  "type": "array",
                  "maxItems": 10,
                  "items": {
                    "type": "string",
                    "maxLength": 64,
                    "minLength": 1
                  }
                }
              },
              "additionalProperties": false
            },
            "clusterLabels": {
              "type": "object",
              "additionalProperties": {
                "type": "string"
              }
            },
            "featureGates": {
              "type": "array",
              "items": {
                "type": "object",
                "required": [
                  "feature"
                ],
                "properties": {
                  "feature": {
                    "type": "string"
                  },
                  "mode": {
                    "type": "string",
                    "default": "Disable",
                    "enum": [
                      "Enable",
                      "Disable"
                    ]
                  }
                },
                "additionalProperties": false
              }
            },
            "kubeAPIBurst": {
              "type": "integer",
              "format": "int32",
              "default": 100
            },
            "kubeAPIQPS": {
              "type": "integer",
              "format": "int32",
              "default": 50
            },
            "registrationDriver": {
              "type": "object",
              "required": [
                "authType"
              ],
              "properties": {
                "authType": {
                  "type": "string",
                  "default": "csr",
                  "enum": [
                    "csr",
                    "awsirsa",
                    "grpc"
                  ]
                },
                "awsIrsa": {
                  "type": "object",
                  "required": [
                    "hubClusterArn",
                    "managedClusterArn"
                  ],
                  "properties": {
                    "hubClusterArn": {
                      "type": "string",
                      "minLength": 1,
                      "pattern": "^arn:aws:eks:([a-zA-Z0-9-]+):(\\d{12}):cluster/([a-zA-Z0-9-]+)$"
                    },
                    "managedClusterArn": {
                      "type": "string",
                      "minLength": 1,
                      "pattern": "^arn:aws:eks:([a-zA-Z0-9-]+):(\\d{12}):cluster/([a-zA-Z0-9-]+)$"
                    }
                  },
                  "additionalProperties": false
                }
              },
              "additionalProperties": false
            }
          },
          "additionalProperties": false
        },
        "registrationImagePullSpec": {
          "type": "string"
        },
        "resourceRequirement": {
          "type": "object",
          "properties": {
            "resourceRequirements": {
              "type": "object",
              "properties": {
                "claims": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "required": [
                      "name"
                    ],
                    "properties": {
                      "name": {
                        "type": "string"
                      },
                      "request": {
                        "type": "string"
                      }
                    },
                    "additionalProperties": false
                  },
                  "x-kubernetes-list-map-keys": [
                    "name"
                  ],
                  "x-kubernetes-list-type": "map"
                },
                "limits": {
                  "type": "object",
                  "additionalProperties": {
                    "pattern": "^(\\+|-)?(([0-9]+(\\.[0-9]*)?)|(\\.[0-9]+))(([KMGTPE]i)|[numkMGTPE]|([eE](\\+|-)?(([0-9]+(\\.[0-9]*)?)|(\\.[0-9]+))))?$",
                    "anyOf": [
                      {
                        "type": "integer"
                      },
                      {
                        "type": "string"
                      }
                    ],
                    "x-kubernetes-int-or-string": true
                  }
                },
                "requests": {
                  "type": "object",
                  "additionalProperties": {
                    "pattern": "^(\\+|-)?(([0-9]+(\\.[0-9]*)?)|(\\.[0-9]+))(([KMGTPE]i)|[numkMGTPE]|([eE](\\+|-)?(([0-9]+(\\.[0-9]*)?)|(\\.[0-9]+))))?$",
                    "anyOf": [
                      {
                        "type": "integer"
                      },
                      {
                        "type": "string"
                      }
                    ],
                    "x-kubernetes-int-or-string": true
                  }
                }
              },
              "additionalProperties": false
            },
            "type": {
              "type": "string",
              "default": "Default",
              "enum": [
                "Default",
                "BestEffort",
                "ResourceRequirement"
              ]
            }
          },
          "additionalProperties": false
        },
        "workConfiguration": {
          "type": "object",
          "properties": {
            "appliedManifestWorkEvictionGracePeriod": {
              "type": "string",
              "pattern": "^([0-9]+(s|m|h))+$"
            },
            "featureGates": {
              "type": "array",
              "items": {
                "type": "object",
                "required": [
                  "feature"
                ],
                "properties": {
                  "feature": {
                    "type": "string"
                  },
                  "mode": {
                    "type": "string",
                    "default": "Disable",
                    "enum": [
                      "Enable",
                      "Disable"
                    ]
                  }
                },
                "additionalProperties": false
              }
            },
            "hubKubeAPIBurst": {
              "type": "integer",
              "format": "int32",
              "default": 100
            },
            "hubKubeAPIQPS": {
              "type": "integer",
              "format": "int32",
              "default": 50
            },
            "kubeAPIBurst": {
              "type": "integer",
              "format": "int32",
              "default": 100
            },
            "kubeAPIQPS": {
              "type": "integer",
              "format": "int32",
              "default": 50
            },
            "statusSyncInterval": {
              "type": "string",
              "pattern": "^([0-9]+(s|m|h))+$"
            }
          },
          "additionalProperties": false
        },
        "workImagePullSpec": {
          "type": "string"
        }
      },
      "additionalProperties": false
    },
    "status": {
      "type": "object",
      "properties": {
        "conditions": {
          "type": "array",
          "items": {
            "type": "object",
            "required": [
              "lastTransitionTime",
              "message",
              "reason",
              "status",
              "type"
            ],
            "properties": {
              "lastTransitionTime": {
                "type": "string",
                "format": "date-time"
              },
              "message": {
                "type": "string",
                "maxLength": 32768
              },
              "observedGeneration": {
                "type": "integer",
                "format": "int64",
                "minimum": 0
              },
              "reason": {
                "type": "string",
                "maxLength": 1024,
                "minLength": 1,
                "pattern": "^[A-Za-z]([A-Za-z0-9_,:]*[A-Za-z0-9_])?$"
              },
              "status": {
                "type": "string",
                "enum": [
                  "True",
                  "False",
                  "Unknown"
                ]
              },
              "type": {
                "type": "string",
                "maxLength": 316,
                "pattern": "^([a-z0-9]([-a-z0-9]*[a-z0-9])?(\\.[a-z0-9]([-a-z0-9]*[a-z0-9])?)*/)?(([A-Za-z0-9][-A-Za-z0-9_.]*)?[A-Za-z0-9])$"
              }
            },
            "additionalProperties": false
          }
        },
        "generations": {
          "type": "array",
          "items": {
            "type": "object",
            "required": [
              "group",
              "lastGeneration",
              "name",
              "resource",
              "version"
            ],
            "properties": {
              "group": {
                "type": "string"
              },
              "lastGeneration": {
                "type": "integer",
                "format": "int64"
              },
              "name": {
                "type": "string"
              },
              "namespace": {
                "type": "string"
              },
              "resource": {
                "type": "string"
              },
              "version": {
                "type": "string"
              }
            },
            "additionalProperties": false
          }
        },
        "observedGeneration": {
          "type": "integer",
          "format": "int64"
        },
        "relatedResources": {
          "type": "array",
          "items": {
            "type": "object",
            "required": [
              "group",
              "name",
              "resource",
              "version"
            ],
            "properties": {
              "group": {
                "type": "string"
              },
              "name": {
                "type": "string"
              },
              "namespace": {
                "type": "string"
              },
              "resource": {
                "type": "string"
              },
              "version": {
                "type": "string"
              }
            },
            "additionalProperties": false
          }
        }
      },
      "additionalProperties": false
    }
  }
}
