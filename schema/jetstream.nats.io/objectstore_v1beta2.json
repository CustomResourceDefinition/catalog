{
  "properties": {
    "spec": {
      "additionalProperties": false,
      "properties": {
        "account": {
          "pattern": "^[^.*>]*$",
          "type": "string"
        },
        "bucket": {
          "type": "string"
        },
        "compression": {
          "type": "boolean"
        },
        "creds": {
          "default": "",
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "maxBytes": {
          "type": "integer"
        },
        "metadata": {
          "additionalProperties": {
            "type": "string"
          },
          "type": "object"
        },
        "nkey": {
          "default": "",
          "type": "string"
        },
        "placement": {
          "additionalProperties": false,
          "properties": {
            "cluster": {
              "type": "string"
            },
            "tags": {
              "items": {
                "type": "string"
              },
              "type": "array"
            }
          },
          "type": "object"
        },
        "preventDelete": {
          "default": false,
          "type": "boolean"
        },
        "preventUpdate": {
          "default": false,
          "type": "boolean"
        },
        "replicas": {
          "default": 1,
          "maximum": 5,
          "minimum": 1,
          "type": "integer"
        },
        "servers": {
          "default": [],
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "storage": {
          "enum": [
            "file",
            "memory"
          ],
          "type": "string"
        },
        "tls": {
          "additionalProperties": false,
          "properties": {
            "clientCert": {
              "type": "string"
            },
            "clientKey": {
              "type": "string"
            },
            "rootCas": {
              "items": {
                "type": "string"
              },
              "type": "array"
            }
          },
          "type": "object"
        },
        "tlsFirst": {
          "default": false,
          "type": "boolean"
        },
        "ttl": {
          "type": "string"
        }
      },
      "type": "object"
    },
    "status": {
      "additionalProperties": false,
      "properties": {
        "conditions": {
          "items": {
            "additionalProperties": false,
            "properties": {
              "lastTransitionTime": {
                "type": "string"
              },
              "message": {
                "type": "string"
              },
              "reason": {
                "type": "string"
              },
              "status": {
                "type": "string"
              },
              "type": {
                "type": "string"
              }
            },
            "type": "object"
          },
          "type": "array"
        },
        "observedGeneration": {
          "type": "integer"
        }
      },
      "type": "object"
    }
  },
  "type": "object"
}
