{
  "properties": {
    "spec": {
      "additionalProperties": false,
      "properties": {
        "account": {
          "pattern": "^[^.*>]*$",
          "type": "string"
        },
        "ackPolicy": {
          "default": "none",
          "enum": [
            "none",
            "all",
            "explicit"
          ],
          "type": "string"
        },
        "ackWait": {
          "default": "1ns",
          "type": "string"
        },
        "backoff": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "creds": {
          "default": "",
          "type": "string"
        },
        "deliverGroup": {
          "type": "string"
        },
        "deliverPolicy": {
          "default": "all",
          "enum": [
            "all",
            "last",
            "new",
            "byStartSequence",
            "byStartTime"
          ],
          "type": "string"
        },
        "deliverSubject": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "durableName": {
          "minLength": 1,
          "pattern": "^[^.*>]+$",
          "type": "string"
        },
        "filterSubject": {
          "type": "string"
        },
        "flowControl": {
          "default": false,
          "type": "boolean"
        },
        "headersOnly": {
          "default": false,
          "type": "boolean"
        },
        "heartbeatInterval": {
          "type": "string"
        },
        "maxAckPending": {
          "type": "integer"
        },
        "maxDeliver": {
          "minimum": -1,
          "type": "integer"
        },
        "maxRequestBatch": {
          "type": "integer"
        },
        "maxRequestExpires": {
          "type": "string"
        },
        "maxRequestMaxBytes": {
          "type": "integer"
        },
        "maxWaiting": {
          "type": "integer"
        },
        "memStorage": {
          "default": false,
          "type": "boolean"
        },
        "nkey": {
          "default": "",
          "type": "string"
        },
        "optStartSeq": {
          "minimum": 0,
          "type": "integer"
        },
        "optStartTime": {
          "type": "string"
        },
        "preventDelete": {
          "default": false,
          "type": "boolean"
        },
        "rateLimitBps": {
          "type": "integer"
        },
        "replayPolicy": {
          "default": "instant",
          "enum": [
            "instant",
            "original"
          ],
          "type": "string"
        },
        "replicas": {
          "type": "integer"
        },
        "sampleFreq": {
          "type": "string"
        },
        "servers": {
          "default": [],
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "streamName": {
          "type": "string"
        },
        "tls": {
          "additionalProperties": false,
          "properties": {
            "clientCert": {
              "type": "string"
            },
            "clientKey": {
              "type": "string"
            },
            "rootCas": {
              "items": {
                "type": "string"
              },
              "type": "array"
            }
          },
          "type": "object"
        }
      },
      "type": "object"
    },
    "status": {
      "additionalProperties": false,
      "properties": {
        "conditions": {
          "items": {
            "additionalProperties": false,
            "properties": {
              "lastTransitionTime": {
                "type": "string"
              },
              "message": {
                "type": "string"
              },
              "reason": {
                "type": "string"
              },
              "status": {
                "type": "string"
              },
              "type": {
                "type": "string"
              }
            },
            "type": "object"
          },
          "type": "array"
        },
        "observedGeneration": {
          "type": "integer"
        }
      },
      "type": "object"
    }
  },
  "type": "object"
}
