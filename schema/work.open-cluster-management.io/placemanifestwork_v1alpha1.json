{
  "type": "object",
  "properties": {
    "apiVersion": {
      "type": "string"
    },
    "kind": {
      "type": "string"
    },
    "metadata": {
      "type": "object"
    },
    "spec": {
      "type": "object",
      "properties": {
        "manifestWorkTemplate": {
          "type": "object",
          "properties": {
            "deleteOption": {
              "type": "object",
              "properties": {
                "propagationPolicy": {
                  "type": "string",
                  "default": "Foreground",
                  "enum": [
                    "Foreground",
                    "Orphan",
                    "SelectivelyOrphan"
                  ]
                },
                "selectivelyOrphans": {
                  "type": "object",
                  "properties": {
                    "orphaningRules": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "required": [
                          "name",
                          "resource"
                        ],
                        "properties": {
                          "group": {
                            "type": "string"
                          },
                          "name": {
                            "type": "string"
                          },
                          "namespace": {
                            "type": "string"
                          },
                          "resource": {
                            "type": "string"
                          }
                        },
                        "additionalProperties": false
                      }
                    }
                  },
                  "additionalProperties": false
                }
              },
              "additionalProperties": false
            },
            "executor": {
              "type": "object",
              "properties": {
                "subject": {
                  "type": "object",
                  "required": [
                    "type"
                  ],
                  "properties": {
                    "serviceAccount": {
                      "type": "object",
                      "required": [
                        "name",
                        "namespace"
                      ],
                      "properties": {
                        "name": {
                          "type": "string",
                          "maxLength": 253,
                          "minLength": 1,
                          "pattern": "^([a-z0-9]([-a-z0-9]*[a-z0-9])?(\\.[a-z0-9]([-a-z0-9]*[a-z0-9])?)*)$"
                        },
                        "namespace": {
                          "type": "string",
                          "maxLength": 253,
                          "minLength": 1,
                          "pattern": "^([a-z0-9]([-a-z0-9]*[a-z0-9])?(\\.[a-z0-9]([-a-z0-9]*[a-z0-9])?)*)$"
                        }
                      },
                      "additionalProperties": false
                    },
                    "type": {
                      "type": "string",
                      "enum": [
                        "ServiceAccount"
                      ]
                    }
                  },
                  "additionalProperties": false
                }
              },
              "additionalProperties": false
            },
            "manifestConfigs": {
              "type": "array",
              "items": {
                "type": "object",
                "required": [
                  "resourceIdentifier"
                ],
                "properties": {
                  "feedbackRules": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "required": [
                        "type"
                      ],
                      "properties": {
                        "jsonPaths": {
                          "type": "array",
                          "items": {
                            "type": "object",
                            "required": [
                              "name",
                              "path"
                            ],
                            "properties": {
                              "name": {
                                "type": "string"
                              },
                              "path": {
                                "type": "string"
                              },
                              "version": {
                                "type": "string"
                              }
                            },
                            "additionalProperties": false
                          }
                        },
                        "type": {
                          "type": "string",
                          "enum": [
                            "WellKnownStatus",
                            "JSONPaths"
                          ]
                        }
                      },
                      "additionalProperties": false
                    }
                  },
                  "resourceIdentifier": {
                    "type": "object",
                    "required": [
                      "name",
                      "resource"
                    ],
                    "properties": {
                      "group": {
                        "type": "string"
                      },
                      "name": {
                        "type": "string"
                      },
                      "namespace": {
                        "type": "string"
                      },
                      "resource": {
                        "type": "string"
                      }
                    },
                    "additionalProperties": false
                  },
                  "updateStrategy": {
                    "type": "object",
                    "required": [
                      "type"
                    ],
                    "properties": {
                      "serverSideApply": {
                        "type": "object",
                        "properties": {
                          "fieldManager": {
                            "type": "string",
                            "default": "work-agent",
                            "pattern": "^work-agent"
                          },
                          "force": {
                            "type": "boolean"
                          }
                        },
                        "additionalProperties": false
                      },
                      "type": {
                        "type": "string",
                        "default": "Update",
                        "enum": [
                          "Update",
                          "CreateOnly",
                          "ServerSideApply"
                        ]
                      }
                    },
                    "additionalProperties": false
                  }
                },
                "additionalProperties": false
              }
            },
            "workload": {
              "type": "object",
              "properties": {
                "manifests": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "x-kubernetes-preserve-unknown-fields": true,
                    "x-kubernetes-embedded-resource": true
                  }
                }
              },
              "additionalProperties": false
            }
          },
          "additionalProperties": false
        },
        "placementRef": {
          "type": "object",
          "properties": {
            "name": {
              "type": "string"
            }
          },
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    },
    "status": {
      "type": "object",
      "properties": {
        "conditions": {
          "type": "array",
          "items": {
            "type": "object",
            "required": [
              "lastTransitionTime",
              "message",
              "reason",
              "status",
              "type"
            ],
            "properties": {
              "lastTransitionTime": {
                "type": "string",
                "format": "date-time"
              },
              "message": {
                "type": "string",
                "maxLength": 32768
              },
              "observedGeneration": {
                "type": "integer",
                "format": "int64",
                "minimum": 0
              },
              "reason": {
                "type": "string",
                "maxLength": 1024,
                "minLength": 1,
                "pattern": "^[A-Za-z]([A-Za-z0-9_,:]*[A-Za-z0-9_])?$"
              },
              "status": {
                "type": "string",
                "enum": [
                  "True",
                  "False",
                  "Unknown"
                ]
              },
              "type": {
                "type": "string",
                "maxLength": 316,
                "pattern": "^([a-z0-9]([-a-z0-9]*[a-z0-9])?(\\.[a-z0-9]([-a-z0-9]*[a-z0-9])?)*/)?(([A-Za-z0-9][-A-Za-z0-9_.]*)?[A-Za-z0-9])$"
              }
            },
            "additionalProperties": false
          }
        },
        "summary": {
          "type": "object",
          "properties": {
            "Applied": {
              "type": "integer"
            },
            "available": {
              "type": "integer"
            },
            "degraded": {
              "type": "integer"
            },
            "progressing": {
              "type": "integer"
            },
            "total": {
              "type": "integer"
            }
          },
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    }
  }
}
