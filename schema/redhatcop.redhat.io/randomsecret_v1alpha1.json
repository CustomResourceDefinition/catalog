{
  "properties": {
    "apiVersion": {
      "type": "string"
    },
    "kind": {
      "type": "string"
    },
    "metadata": {
      "type": "object"
    },
    "spec": {
      "additionalProperties": false,
      "properties": {
        "authentication": {
          "additionalProperties": false,
          "properties": {
            "namespace": {
              "type": "string"
            },
            "path": {
              "default": "kubernetes",
              "pattern": "^(?:/?[\\w;:@&=\\$-\\.\\+]*)+/?",
              "type": "string"
            },
            "role": {
              "type": "string"
            },
            "serviceAccount": {
              "additionalProperties": false,
              "default": {
                "name": "default"
              },
              "properties": {
                "name": {
                  "type": "string"
                }
              },
              "type": "object",
              "x-kubernetes-map-type": "atomic"
            }
          },
          "type": "object"
        },
        "connection": {
          "additionalProperties": false,
          "properties": {
            "address": {
              "type": "string"
            },
            "maxRetries": {
              "type": "integer"
            },
            "tLSConfig": {
              "additionalProperties": false,
              "properties": {
                "cacert": {
                  "type": "string"
                },
                "skipVerify": {
                  "type": "boolean"
                },
                "tlsSecret": {
                  "additionalProperties": false,
                  "properties": {
                    "name": {
                      "type": "string"
                    }
                  },
                  "type": "object",
                  "x-kubernetes-map-type": "atomic"
                },
                "tlsServerName": {
                  "type": "string"
                }
              },
              "type": "object"
            },
            "timeOut": {
              "type": "string"
            }
          },
          "type": "object"
        },
        "isKVSecretsEngineV2": {
          "default": false,
          "type": "boolean"
        },
        "kvSecretRetainPolicy": {
          "default": "Delete",
          "enum": [
            "Delete",
            "Retain"
          ],
          "type": "string"
        },
        "name": {
          "pattern": "[a-z0-9]([-a-z0-9]*[a-z0-9])?",
          "type": "string"
        },
        "path": {
          "pattern": "^(?:/?[\\w;:@&=\\$-\\.\\+]*)+/?",
          "type": "string"
        },
        "refreshPeriod": {
          "type": "string"
        },
        "secretFormat": {
          "additionalProperties": false,
          "properties": {
            "inlinePasswordPolicy": {
              "type": "string"
            },
            "passwordPolicyName": {
              "type": "string"
            }
          },
          "type": "object"
        },
        "secretKey": {
          "type": "string"
        }
      },
      "type": "object"
    },
    "status": {
      "additionalProperties": false,
      "properties": {
        "conditions": {
          "items": {
            "additionalProperties": false,
            "properties": {
              "lastTransitionTime": {
                "format": "date-time",
                "type": "string"
              },
              "message": {
                "maxLength": 32768,
                "type": "string"
              },
              "observedGeneration": {
                "format": "int64",
                "minimum": 0,
                "type": "integer"
              },
              "reason": {
                "maxLength": 1024,
                "minLength": 1,
                "pattern": "^[A-Za-z]([A-Za-z0-9_,:]*[A-Za-z0-9_])?$",
                "type": "string"
              },
              "status": {
                "enum": [
                  "True",
                  "False",
                  "Unknown"
                ],
                "type": "string"
              },
              "type": {
                "maxLength": 316,
                "pattern": "^([a-z0-9]([-a-z0-9]*[a-z0-9])?(\\.[a-z0-9]([-a-z0-9]*[a-z0-9])?)*/)?(([A-Za-z0-9][-A-Za-z0-9_.]*)?[A-Za-z0-9])$",
                "type": "string"
              }
            },
            "required": [
              "lastTransitionTime",
              "message",
              "reason",
              "status",
              "type"
            ],
            "type": "object"
          },
          "type": "array",
          "x-kubernetes-list-map-keys": [
            "type"
          ],
          "x-kubernetes-list-type": "map"
        },
        "lastVaultSecretUpdate": {
          "format": "date-time",
          "type": "string"
        }
      },
      "type": "object"
    }
  },
  "type": "object"
}
